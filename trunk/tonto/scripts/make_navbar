#!/bin/csh -f
## make_navbar
## ! $Id$

echo '<HTML>'  > TONTO-nav-bar.html
echo '<HEAD>' >> TONTO-nav-bar.html
echo '  <META HTTP-EQUIV="Content-Type" CONTENT="text/html; charset=iso-8859-1">' >> TONTO-nav-bar.html
echo '  <META name="robots" content="noindex, nofollow">' >> TONTO-nav-bar.html
echo '  <TITLE>Tonto Navigation</TITLE>' >> TONTO-nav-bar.html
echo '<BASE TARGET="main">' >> TONTO-nav-bar.html
echo '</HEAD>' >> TONTO-nav-bar.html
echo '<body BGCOLOR="#FFFBF0">' >> TONTO-nav-bar.html
echo '<BR>' >> TONTO-nav-bar.html
echo '<DIV ALIGN=CENTER>' >> TONTO-nav-bar.html
echo '</A><IMG SRC="tonto_nav.gif" HEIGHT=61 WIDTH=113 BORDER="1"></A>' >> TONTO-nav-bar.html
echo '<DL COMPACT>' >> TONTO-nav-bar.html

echo '<BR>' >> TONTO-nav-bar.html
echo '<HR NOSHADE WIDTH=80%>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="WWW/index.html">What is TONTO?</A>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="foofiles">.foo source files</A>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="f90files">.f90 source files</A>' >> TONTO-nav-bar.html

echo '<BR>' >> TONTO-nav-bar.html
echo '<HR NOSHADE WIDTH=80%>' >> TONTO-nav-bar.html
echo '<BR>SCRIPTS AND UTILITIES<BR><BR>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="scripts/macros">macros</A>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="scripts/Makefile">Makefile</A>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="scripts/make_dependfile">make_dependfile</A>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="scripts/make_dependfile.script">make_dependfile.script</A>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="scripts/update_module_file">update_module_file</A>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="scripts/foo.perl">foo.perl</A>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="scripts/foo2html">foo2html</A>' >> TONTO-nav-bar.html
echo '      <DT><A HREF="scripts/foo2html.script">foo2html.script</A>' >> TONTO-nav-bar.html
echo '<BR>' >> TONTO-nav-bar.html
echo '<HR NOSHADE WIDTH=80%>' >> TONTO-nav-bar.html
echo '<BR>MODULES<BR><BR>' >> TONTO-nav-bar.html

foreach i ($argv) 
   echo '<DT><A HREF="htmlfiles/'$i'_short.html">'$i'</A> <A HREF="foofiles/'$i'.foo">.foo</A>' >> TONTO-nav-bar.html
end

echo '</DL>'   >> TONTO-nav-bar.html
echo '</DIV>'  >> TONTO-nav-bar.html
echo '</BODY>' >> TONTO-nav-bar.html
echo '</HTML>' >> TONTO-nav-bar.html
