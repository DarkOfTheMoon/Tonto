                        H    .255700   .755700   .284100    .044000   .044000   .021600  -.022200  -.003100  -.003100
                        H   -.255700   .244300   .284100    .044000   .044000   .021600  -.022200   .003100   .003100
                        H    .143100   .643100  -.034800    .043000   .043000   .014000  -.015800   .001900   .001900
                        H   -.143100   .356900  -.034800    .043000   .043000   .014000  -.015800  -.001900  -.001900
 
                }
        }

        ! What basis set?
   
        basis_name= STO-3G


        ! Charge multiplicity

        charge= 0

        multiplicity= 1


        ! SCF information

        scfdata= {
         
                kind=           xray_rhf

                initial_density= promolecule

                direct=         on

                initial_lambda= 0.000
                lambda_max=     0.001
                lambda_step=    0.001

                convergence=       0.00001
                diis= { convergence_tolerance= 0.00001 }

                cluster_radius= 20 ! for cluster calc of crystal E field

        }


   ! Reset output format

   output_style_options= {
      real_precision= 3
      real_width= 11
   }

   ! Put output
  
   put


   ! Do SCF

   scf

   ! Integration grid

   becke_grid= {
     accuracy= high
   }


   ! Print properties

   put_1e_properties


   ! Print the ELF

   plot_grid= {

      kind= ELF
      centre_atom= 4
      x_axis_atoms= 4 1
      y_axis_atoms= 4 2
      x_width= 10 angstrom
      y_width= 10 angstrom
      n_points= 101

   }

   plot

   delete_plot_grid ELF_grid
   delete_scf_archives
   delete_scf_integrals


}

 
