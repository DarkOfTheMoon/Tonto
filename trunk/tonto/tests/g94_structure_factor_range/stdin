{

   name=     nh2cn
   
   output_style_options= {
      real_precision= 4
   }
   
   read_gaussian_fchk_file nh2cn_b3lyp.fchk
   
   charge=  0
   
   multiplicity= 1
   
   crystal= {
   
     spacegroup= { HM_symbol=   "P b c a" }
   
     unit_cell= {
       angles=      90.0   90.0   90.0       Degree
       dimensions=   6.856       6.628       9.147     angstrom
     } 
   
     xray_data= {

       thermal_smearing_model= stewart
       partition_model= mulliken
       optimise_extinction= true
   
       reflection_data=  {
         do {
            set_hkl_range= { { -1 1 } { 3 5 } { 0 3 } }
         }
       } 

     }

   }
    

   atoms= {
   
      keys= { 
         label= 
         { thermal_axis_system= crystal  units= angstrom^2 }  
         thermal_tensor= 
      }
   
      data= {
         N  0.02687 0.02687 0.02687  0.0  0.0  0.0
         N  0.02483 0.02483 0.02483  0.0  0.0  0.0
         C  0.01945 0.01945 0.01945  0.0  0.0  0.0
         H  0.0385  0.0385  0.0385   0.0  0.0  0.0
         H  0.0603  0.0603  0.0603   0.0  0.0  0.0
      }
   }
   
   make_structure_factors
   
   put
   
   delete_scf_archives
   delete_scf_integrals

}
