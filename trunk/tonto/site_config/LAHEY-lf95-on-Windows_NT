FOPTNS     = $(FFAST)
FFLAGS     = -mod $(moddir) -I $(srcdir) -I $(moddir) -I $(objdir)
FPROF      = -p 
FDEBUG     = -g 
FFAST      = -O1
FSUFFIX    = F90
FOUTSYNTAX = -out
OBJSUFFIX  = OBJ
PROGSUFFIX = EXE
MKLIB      = lib
LIBSUFFIX  = lib
DEFS       = -DFLUSH -DUSE_PRE_AND_POST_CONDITIONS
LIBOUT     = /OUT:
