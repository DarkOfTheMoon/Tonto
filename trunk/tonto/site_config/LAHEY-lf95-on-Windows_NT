FOPTNS     = $(FFAST)
FFLAGS     = -mod $(moddir) -I $(srcdir) -I $(moddir) -I $(objdir)
FPROF      = -p 
FDEBUG     = -g 
FFAST      = -O1
FSUFFIX    = F90
OBJSUFFIX  = obj
OBJOUT     = -o
PROGSUFFIX = exe
PROGOUT    = -out
LIBOUT     = /OUT:
LIBSUFFIX  = lib
MKLIB      = lib
DEFS       = -DFLUSH -DUSE_PRE_AND_POST_CONDITIONS
