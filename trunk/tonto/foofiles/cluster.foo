      u :: INT, IN
      symop :: MAT{REAL}, OUT
      par :: VEC{INT}, OUT
      pos :: MAT{REAL}, OUT
      charge :: VEC{REAL}, OUT
      mass   :: VEC{REAL}, OUT
      dipole :: MAT{REAL}, OUT
      alpha_q :: MAT{REAL}, OUT
      alpha_m :: MAT3{REAL}, OUT

      occ_list :: VEC{INT}*

      occ_list.create(1)
      occ_list(1) = 5 + 5*10 + 5*100 + u*1000

      .do_defragment(occ_list)
