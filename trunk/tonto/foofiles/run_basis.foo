program run_basis
! $Id$

   use TYPES
   use SYSTEM
   use TEXTFILE
   use BASIS

#include "macros"

   implicit none

   TEXTFILE* :: in,out
   BASIS :: b

   tonto.initialize

   in.create("input")
   in.open_for_read
   stdin => in

   out.create("output")
   out.open_for_write
   stdout => out

   b.read
   b.put

end
