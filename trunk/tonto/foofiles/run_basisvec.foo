program run_basisvec
! $Id$

   use TYPES
   use SYSTEM
   use TEXTFILE
   use BASISVEC

#include "macros"

   implicit none

   TEXTFILE* :: in,out
   BASISVEC* :: b

   tonto.initialize

   in.create("input")
   in.open_for_read
   stdin => in

   out.create("output")
   out.open_for_write
   stdout => out

   b.read
   b.put

end
