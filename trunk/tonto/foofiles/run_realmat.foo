program run_mat

! $Id$

  use TYPES
  use SYSTEM
  use VEC
  use MAT
  use TEXTFILE

# include "macros"

  implicit none
  
  MAT* :: matrix
  MAT* :: eigenvectors
  VEC* :: eigenvalues
  TEXTFILE* :: in, out

  tonto.initialize

  matrix.create(2,2)
  eigenvectors.create(2,2)
  eigenvalues.create(2)

  matrix = ONE

  matrix.solve_eigenproblem(eigenvalues,eigenvectors)

  write(*,*) eigenvalues

end 

