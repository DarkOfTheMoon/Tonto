program run_time
! $Id$

   use MM
   use OUTPUT
   use TIME

#include "macros"

   implicit none

   TIME :: timer
   INT :: i
   DBL :: j
   IVEC(8) :: times

   timer.start
   std_output.create("time.out")
   std_output.open
   timer.set_output_file(std_output)

   timer.output_current_time

   j=0d0
   do i=1,100000000
     j=j+1d0
   end

   timer.stop

end 
