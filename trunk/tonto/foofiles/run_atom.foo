program run_atom
! $Id$

   use TYPES
   use SYSTEM
   use TEXTFILE
   use ATOM

#include "macros"

   implicit none

   TEXTFILE* :: in,out
   ATOM :: a

   tonto.initialize 

   in.create("input")
   in.open_for_read
   stdin => in

   out.create("output")
   out.open_for_write
   stdout => out

   a.read
   a.put

end 
