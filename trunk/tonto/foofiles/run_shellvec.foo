program run_shellvec
! $Id$

   use TYPES
   use SYSTEM
   use SHELL
   use SHELLVEC
   use TEXTFILE

#include "macros"

   implicit none

   TEXTFILE* :: in,out
   SHELLVEC* :: sh

   tonto.initialize

   in.create("input")
   in.open_for_read
   stdin => in

   out.create("output")
   out.open_for_write
   stdout => out

   sh.read
   sh.put

end
