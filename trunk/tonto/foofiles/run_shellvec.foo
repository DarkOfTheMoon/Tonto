program run_shellvec

   use TYPES
   use SYSTEM
   use SHELL
   use SHELLVEC
   use IMAT
   use INPUT
   use OUTPUT

#include "macros"

   implicit none

   INPUT, PTR :: in
   OUTPUT, PTR :: out
   SHELLVEC, PTR :: sh

   out.create("output")
   out.open
   in.create("input")
   in.open

   sh.read(in)
   sh(1).put(out)
   sh(2).put(out)

end
