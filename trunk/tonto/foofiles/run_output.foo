program run_output
! $Id$

   use SYSTEM
   use OUTPUT
   use TIME

#include "macros"

   implicit none

   TIME :: timer

   tonto.initialize

   timer.start
   std_output.create("output.out")
   std_output.open
   timer.set_output_file(std_output)
   timer.output_current_time

   std_output.test

   timer.stop
end 
