program run_SHELL1QUARTET

   implicit none

   s  :: SHELL4*
   s4 :: SHELL1QUARTET*
   ab,cd :: SHELL1PAIR*
   I1,I2,I3 :: MAT4{REAL}*
   al,bl,cl,dl :: INT
   maxi :: REAL

   TONTO_CREATE

   stdout.create_stdout; stdout.open

   stdout.set_real_style("f")
   stdout.set_real_precision(9)
   stdout.set_real_width(15)
   stdout.set_int_width(3)

   GAUSSIAN_DATA:set_indices(2*4)

   stdout.flush
   stdout.text("ERI integrals for various shell4's, to test all shell4 ERI routines")

   do al=0,4
     do bl=0,4
       do cl=0,4
         do dl=0,4

           s.create

           s.a.n_cc = 2
           s.b.n_cc = 2
           s.c.n_cc = 2
           s.d.n_cc = 2
           s.a.cc.create(2)
           s.b.cc.create(2)
           s.c.cc.create(2)
           s.d.cc.create(2)
           s.a.cc(1) = ONE
           s.b.cc(1) = ONE
           s.c.cc(1) = THREE
           s.d.cc(1) = ONE
           s.a.cc(2) = ONE
           s.b.cc(2) = THREE
           s.c.cc(2) = TWO
           s.d.cc(2) = TWO

           s.a.ex.create(2)
           s.b.ex.create(2)
           s.c.ex.create(2)
           s.d.ex.create(2)
           s.a.ex(1) = 2d0
           s.b.ex(1) = 1d0
           s.c.ex(1) = 50d0
           s.d.ex(1) = 0.1d0
           s.a.ex(2) = 20d0
           s.b.ex(2) = 10d0
           s.c.ex(2) = 5d0
           s.d.ex(2) = 1d0

           s.a.set_l(al)
           s.b.set_l(bl)
           s.c.set_l(cl)
           s.d.set_l(dl)
           s.a.set_n_comp
           s.b.set_n_comp
           s.c.set_n_comp
           s.d.set_n_comp

           s.a.pos = [0.0d0, 0.2d0, 0.0d0]
           s.b.pos = [0.3d0,-0.3d0, 0.6d0]
           s.c.pos = [0.8d0, 0.5d0, 0.2d0]
           s.d.pos = [0.0d0, 0.2d0,-0.3d0]

           ab.create
           cd.create
           ab.copy_a(s.a)
           ab.copy_b(s.b)
           cd.copy_a(s.c)
           cd.copy_b(s.d)
           ab.make_precalculated_data
           cd.make_precalculated_data

           s4.create
           s4.set_ab_new(ab,s.a.pos,s.b.pos,same_center=FALSE,skip=FALSE)
           s4.set_cd_new(cd,s.c.pos,s.d.pos,same_center=FALSE,skip=FALSE)

           I1.create(s.a.n_comp,s.b.n_comp,s.c.n_comp,s.d.n_comp)
           I2.create(s.a.n_comp,s.b.n_comp,s.c.n_comp,s.d.n_comp)
           I3.create(s.a.n_comp,s.b.n_comp,s.c.n_comp,s.d.n_comp)

           stdout.text("*************************************************************")
           s.put
           stdout.flush
           stdout.text("Calculated using Roland Lindh's method:")
           stdout.flush

           s.get_ERI(I1)
           stdout.put(I1)
           stdout.flush

           stdout.text("Calculated using the HONDO Rys method:")
           stdout.flush
           s.make_ERI_ints(I2)
           stdout.put(I2)
           stdout.flush

           stdout.text("Calculated using SHELL1QUARTET method:")
           stdout.flush
           s.make_ERI_ints(I3)
           stdout.put(I2)
           stdout.flush

           maxi = maxval(abs(I1-I2))
           stdout.set_real_style("d")
           stdout.show("Maximum difference, Lindh - HONDO         = ",maxi)
           stdout.set_real_style("f")
           stdout.flush

           maxi = maxval(abs(I1-I3))
           stdout.set_real_style("d")
           stdout.show("Maximum difference, Lindh - SHELL1QUARTET = ",maxi)
           stdout.set_real_style("f")
           stdout.flush

           maxi = maxval(abs(I2-I2))
           stdout.set_real_style("d")
           stdout.show("Maximum difference, HONDO - SHELL1QUARTET = ",maxi)
           stdout.set_real_style("f")
           stdout.flush

           I3.destroy
           I2.destroy
           I1.destroy
           cd.destroy
           ab.destroy
           s4.destroy
           s.destroy

         end
       end
     end
   end

   TONTO_REPORT
   TEXTFILE:destroy(stdout)
   TONTO_DESTROY

end

