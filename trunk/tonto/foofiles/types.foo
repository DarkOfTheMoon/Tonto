   type mol_type
      STR             :: name                    ! Name of molecule
      INPUT, PTR      :: in => NULL()            ! Input file
      OUTPUT, PTR     :: out => NULL()           ! Output file
      VEC(3)          :: B_field                 ! Applied magnetic field in atomic units
      VEC(3)          :: E_field                 ! Applied electric field in atomic units
      XTAL, PTR       :: xtal => NULL()          ! Crystal symmetry of the enclosing crystal
      POINTGROUP, PTR :: pointgroup => NULL()    ! Pointgroup symmetry of the molecule
      ATOMVEC, PTR    :: atom => NULL()          ! List of atoms
      BASISVEC, PTR   :: basis => NULL()         ! List of basis sets
      PLOTGRID, PTR   :: grid => NULL()          ! An enclosing rectangular grid for the molecule, for plots
      DFTGRID, PTR    :: dftgrid => NULL()       ! For storing and generating DFT integration grids
      INT :: charge                              ! Electric charge molecule
      INT :: mult                                ! Spin multiplicity of molecule
      INT :: thermal_smearing_model              ! Thermal smearing model for ft integrals
      BIN :: optimise_thermals                   ! Whether to optimise the thermal parameters
      INT :: n_e                                 ! No of electrons
      INT :: n_a                                 ! No of alpha electrons
      INT :: n_b                                 ! No of beta electrons
      INT :: n_atom                              ! No of atoms
      INT :: n_basis                             ! No of basis sets
      INT :: n_shell                             ! Total number of shells in the molecular basis set
      INT :: n_shell_pairs                       ! Total number of shell pairs in the molecular basis set
      INT :: n_shell_quartets                    ! Total number of shell quartets in the molecular basis set
      INT :: n_bf                                ! Total number of basis functions in the molecular basis set
      INT :: n_prim                              ! Total number of primitives in the molecular basis set
      IVEC, PTR    :: atom_for_shell => NULL()   ! Atom index for molecular shell index array
      IVEC, PTR    :: atom_shell_for_shell => NULL()      ! Atom shell index for molecular shell index array
      IVEC, PTR    :: first_shell_for_atom => NULL()      ! First molecule shell index for an atom
      IVEC, PTR    :: first_basis_fn_for_shell => NULL()  ! First basis function for a given shell
      IVEC, PTR    :: last_basis_fn_for_shell => NULL()   ! Last basis function for a given shell
      SCFDATA, PTR :: scfdata => NULL()          ! SCF data object
      OPVEC, PTR   :: orbital_energies => NULL() ! The orbital energies
      OPMAT, PTR   :: molecular_orbitals => NULL() ! The (real) molecular orbitals
      OPMAT, PTR   :: density_matrix => NULL()   ! The real density matrix
      OPMAT, PTR   :: natural_orbitals => NULL() ! The natural orbitals
      OPVEC, PTR   :: occupation => NULL()       ! The associated occupation numbers
      OPMAT, PTR   :: fock_matrix => NULL()      ! The real fock matrix
      STR          :: partition_model            ! Model for partitioning fragments of the molecule
      VEC, PTR     :: partition_factors => NULL()         ! Partitioning factors for each atom
   end 

