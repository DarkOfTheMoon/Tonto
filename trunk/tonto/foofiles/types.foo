      IVEC, PTR :: k                                    ! Miller indices for the reflections
      IVEC, PTR :: l                                    ! Miller indices for the reflections
      VEC, PTR :: F_exp                                 ! Experimental structure factors
      VEC, PTR :: sigma                                 ! Experimental sigma
