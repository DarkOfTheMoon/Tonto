   same_as(b) result(res) 
   ! Return TRUE if the basis set "self" is the same as "b". Only the
   ! shell vector is compared to see if they are "really" the same.
      self :: IN
      b :: GEMINAL_WAVEFUNCTION, IN
      res :: BIN
      res = FALSE
   end

!  ***************
!  Integal methods
!  ***************

   do_overlap
   ! Evaluate the overlap integral
      res :: REAL
      res = .overlap
      stdout.show("The result is =",res)
   end

   overlap result (res)
   ! Evaluate the overlap integral
      res :: REAL
   ENSURE(.bra.dim==.ket.dim,"Bra and ket have different sizes!")
      i :: INT
      reorder :: VEC{INT}*
      !indices   indices = 0
      !indices   ind = 0
      reorder.create(.n_geminals)
      reorder = [ (i, i=1,.n_geminals) ]
      res = .overlap_for(reorder)
      reorder.destroy
   end

   overlap_for(order) result (res) ::: recursive
   ! Evaluate the overlap integral
      order :: VEC{INT}
      res :: REAL
  ! not relevant anymore since this not changing and it has already been tested
  ! ENSURE(.bra.dim==.ket.dim,"self bra and ket have different sizes!")
      ii,jj,i,j,k,dim, bi1,bi2,bj1,bj2 :: INT
      fac,this,this1,this2 :: REAL
      reorder :: VEC{INT}*
      dim = order.dim
     !debug write(*,*) "dim =",dim

      res = ZERO
      
      if (dim==0) then
         res = ONE
!indices            stdout.text("indices:")
!indices            stdout.put(indices)
         return
      end
          !debug        stdout.text("Entering routine ....")
          !debug        stdout.text("special_bra:")
          !debug        stdout.put(.special_bra)
          !debug        stdout.text("special_ket:")
          !debug        stdout.put(.special_ket)
      do ii = dim,1,-1
         i = order(ii)
      !debug   write(*,*) "i =",i
         ! The simple case 1
         if (.special_ket(i)[1]>0) then
            bi1 = .special_ket(i)[1]
            bi2 = .special_ket(i)[2]
            this = .bra(dim)[bi1,bi2]
         else
            this = sum(.bra(dim)[:,:]*.ket(i)[:,:])
         end
         if (NOT this.is_zero) then
     !debug write(*,*) "again, dim =",dim
             reorder.create(dim-1)
             reorder(1:ii-1)   = [ (order(j), j=1,ii-1) ]
             reorder(ii:dim-1) = [ (order(j), j=ii+1,dim) ]
                     stdout.show("i =",i)
                     stdout.show("dim =",dim)
         if (.special_ket(i)[1]>0) then
                     stdout.show("bi1 =",bi1)
                     stdout.show("bi2 =",bi2)
         end
                     stdout.show("this =",this)
             !indices   ind = ind + 1
             !indices   indices(ind,1) = i
             !indices   indices(ind,2) = i
             !indices   indices(ind,3) = dim
             res = res + .overlap_for(reorder) * this ! <<<<<<<<<<<<<<<<<<<<<<<
             !indices   ind = ind - 1
             !indices   indices(ind,1) = 0
             !indices   indices(ind,2) = 0
             !indices   indices(ind,3) = 0
             reorder.destroy
         end
         ! Case 2
         do jj = dim,ii+1,-1
            j = order(jj)
            if (.special_ket(i)[1]>0 AND .special_ket(j)[1]>0) then
              !       stdout.text("Case 2, i special, j special")
                  bi1 = .special_ket(i)[1]
                  bi2 = .special_ket(i)[2]
                  bj1 = .special_ket(j)[1]
                  bj2 = .special_ket(j)[2]
                  this1 = .bra(dim)[bi1,bj2]
                  this2 = .bra(dim)[bj1,bi2]
                  if (NOT this1.is_zero OR NOT this2.is_zero) then
                    reorder.create(dim-1)
                    reorder(1:ii-1) = [ (order(k), k=1,ii-1) ]
                    reorder(ii:jj-2) = [ (order(k), k=ii+1,jj-1) ]
                    reorder(jj-1:dim-2) = [ (order(k), k=jj+1,dim) ]
                    reorder(dim-1) = i ! This is not used, put to have same dim as in case 1
                    if (NOT this1.is_zero) then
                      .special_ket(i)[1] = bj1 ! i=reorder(dim-1)
                  !   .special_ket(i)[2] = bi2 
                     stdout.show("i =",i)
                     stdout.show("j =",j)
                     stdout.show("dim =",dim)
                     stdout.show("bi1 =",bi1)
                     stdout.show("bj2 =",bj2)
                     stdout.show("this1 =",this1)
        !debug write(*,*) "again 4, dim =",dim
             !debug     stdout.text("AFter assignment ...")
             !debug     stdout.show("i =",i)
             !debug     stdout.show("j =",j)
             !debug     stdout.show("bi1 =",.special_ket(i)[1])
             !debug     stdout.show("bi2 =",.special_ket(i)[2])
             !debug     stdout.text("reorder:")
             !debug     stdout.put(reorder)
             !debug     stdout.text("special_bra:")
             !debug     stdout.put(.special_bra)
             !debug     stdout.text("special_ket:")
             !debug     stdout.put(.special_ket)
                  !indices   ind = ind + 1
                  !indices   indices(ind,1) = i
                  !indices   indices(ind,2) = j
                  !indices   indices(ind,3) = dim
       !debug  write(*,*) "A. recursing here, dim =",dim
                  res = res - .overlap_for(reorder) * this1 ! <<<<<<<<<<<<<<<<<<<<<<<
       !debug  write(*,*) "returned from recursion, dim =",dim
                  !indices   ind = ind - 1
                  !indices   indices(ind,1) = 0
                  !indices   indices(ind,2) = 0
