                  !indices   indices(ind,3) = 0
                      end
                      .special_ket(i)[1] = 0 ! i=reorder(dim-1)
                      .special_ket(i)[2] = 0 ! i=reorder(dim-1)
                      reorder.destroy
                    end
                  end 
               end
               end
               end
               end
            end
         end
      end
   end

