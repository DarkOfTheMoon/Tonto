!   add_r_exchange_functional(p,np,f)
!   ! Calculate the exchange functional.
!   ! Input is p,np.  Output is f.
!   ! np is optional.
!   ! (p means density, np means gradient of the density).
!   !
!   ! NOTE: When adding a new functional, alter the subroutine
!   !       scfdata.foo/read_dft_exchange also.
!   !
!     p :: VEC{REAL}, IN
!     f :: VEC{REAL}
!     np :: MAT{REAL}, optional, IN
!     select case (.scfdata.dft_exchange_functional)
!       case ("none")
!       case ("slater");  .dftgrid.add_r_LDA_x_functional(p,f)
!       case ("xalpha");  .dftgrid.add_r_Xalpha_x_functional(p,f,0.7d0)
!       case ("becke88"); .dftgrid.add_r_Becke88_x_functional(p,np,f)
!     ! case ("gill96")
!     !   ENSURE(present(np),"program bug!")
!     !   .dftgrid.r_gill96_exchange_functional(p,np,f)
!       case default
!         DIE("unknown exchange functional")
!     end
!   end

!   add_r_correlation_functional(p,np,f)
!   ! Calculate the exchange correlation.
!   ! Input is p,np.  Output is f.
!   ! np are optional.
!   ! (p means density, np means gradient of the density).
!   !
!   ! NOTE: When adding a new functional, alter the subroutine
!   !       scfdata.foo/read_dft_correlation also.
!   !
!     p :: VEC{REAL}, IN
!     f :: VEC{REAL}
!     np :: MAT{REAL}, optional, IN
!     select case (.scfdata.dft_correlation_functional)
!       case ("none")
!       case ("lyp"); .dftgrid.add_r_LYP_c_functional(p,np,f)
!       case default; DIE("unknown exchange functional")
!     end
!   end

   add_r_XC_functionals(E,rho0,rho1)
   ! Return the *restricted* exchange correlation functional "E", for given
   ! densities "rho0" and gradient densities "rho1".
      E    :: VEC{REAL}, OUT
      rho0 :: VEC{REAL}, IN
      rho1 :: MAT{REAL}, IN, optional
   ENSURE(.scfdata.created,"no scfdata")
      exch,corr :: STR
      add :: DFT_FUNCTIONAL
      if (NOT .scfdata.is_DFT_calculation) return
      exch = .scfdata.dft_exchange_functional
      corr = .scfdata.dft_correlation_functional
      if (exch/="none") add.r_functional(exch,E,rho0,rho1)
      if (corr/="none") add.r_functional(corr,E,rho0,rho1)
   end

   set_r_XC_energy_density(E,rho0,rho1)
   ! Set the *restricted* exchange correlation energy density "E", for given
   ! densities "rho0" and gradient densities "rho1".
      E    :: VEC{REAL}, OUT
      rho0 :: VEC{REAL}, IN
      rho1 :: MAT{REAL}, IN, optional
   ENSURE(.scfdata.created,"no scfdata")
      exch,corr :: STR
      add :: DFT_FUNCTIONAL
      if (NOT .scfdata.is_DFT_calculation) return
      exch = .scfdata.dft_exchange_functional
      corr = .scfdata.dft_correlation_functional
      E = ZERO ! <<< set to zero
      if (exch/="none") add.r_energy_density(exch,E,rho0,rho1)
      if (corr/="none") add.r_energy_density(corr,E,rho0,rho1)
   end

   add_r_XC_potentials(V0,rho0,V1,rho1)
   ! For given densities "rho0" and gradient densities "rho1", return "V0" and
   ! "V1", the local and non_local terms necessary to calculate the matrix
   ! elements of the *restricted* exchange correlation potential. 
      V0   :: VEC{REAL}, OUT
      rho0 :: VEC{REAL}, IN
      V1   :: MAT{REAL}, OUT, optional
      rho1 :: MAT{REAL}, IN, optional
   ENSURE(.scfdata.created,"no scfdata")
      exch,corr :: STR
      add :: DFT_FUNCTIONAL
      if (NOT .scfdata.is_DFT_calculation) return
      exch = .scfdata.dft_exchange_functional
      corr = .scfdata.dft_correlation_functional
      if (exch/="none") add.r_potential(exch,V0,rho0,V1,rho1)
      if (corr/="none") add.r_potential(corr,V0,rho0,V1,rho1)
   end

!   add_r_XC_potentials_for(dft_functional,rho0,V0,rho1,V1) ::: private
!   ! For a given "dft_functional", and for given densities "rho0" and gradient
!   ! densities "rho1", return "V0" and "V1", the local and non_local terms
!   ! necessary to calculate the matrix elements of the *restricted* exchange
!   ! correlation potential. The potentials are defined as:
!   !    V0 = d F/d rho_a
!   !    V1 = 2*(d F/d gamma_aa) + (d F/d gamma_ab)
!   ! where gamma_xy = nabla rho_x . nabla rho_y, and x,y stand for alpha or
!   ! beta. NOTE: the derivatives are w.r.t. rho_a, or gamma_aa, but the inputs
!   ! are the *restricted* density "rho0" and its gradient "rho1". NOTE: "V1"
!   ! is integrated with nabla of the basis function pair to get the full matrix
!   ! element. "rho1" and "V1" do not appear for non-GGA density functionals.
!     dft_functional :: STR, IN
!     rho0 :: VEC{REAL}, IN
!     V0   :: VEC{REAL}, OUT
!     rho1 :: MAT{REAL}, IN, optional
!     V1   :: MAT{REAL}, OUT, optional
!     select case (dft_functional)
!       case ("none   ")  ! Don't add anything.
!       case ("becke88"); .dftgrid.add_r_Becke88_x_potential(rho0,rho1,V0,V1)
!       case ("lyp    "); .dftgrid.add_r_LYP_c_potential(rho0,rho1,V0,V1)
!       case ("slater "); .dftgrid.add_r_LDA_x_potential(rho0,V0)
!       case ("xalpha "); .dftgrid.add_r_Xalpha_x_potential(rho0,V0,0.7d0)
!       case default;     DIE("unknown exchange functional")
!     end
!   end

!   add_r_exchange_matrix(p,local,np,non_local)
!   ! Get terms necessary to calculate the exchange functional matrix.  "p" and
!   ! "np" are input density and gradient density, respectively, evaluated on the
!   ! grid points.  "local" and "non_local" are the output functional derivatives
!   ! w.r.t. the density and gradient density, respectively. The "non_local" term
!   ! must be integrated with nabla of the basis function pair to get the full
!   ! matrix element.
!     p,local :: VEC{REAL}
!     np,non_local :: MAT{REAL}, optional
!     l :: VEC{REAL}*
!     nl :: MAT{REAL}*
!     alpha :: REAL
!     n_pt :: INT
!     n_pt = size(p)
!     select case (.scfdata.dft_exchange_functional)
!       case ("none")
!       ! Don't add anything.
!       case ("slater");  .dftgrid.add_r_LDA_x_potential(p,local)
!       case ("xalpha");  .dftgrid.add_r_Xalpha_x_potential(p,local,0.7d0)
!       case ("becke88"); .dftgrid.add_r_Becke88_x_potential(p,np,local,non_local)
!     ! case ("gill96")
!     !   ENSURE(present(np),"program bug!")
!     !   ENSURE(present(non_local),"program bug!")
!     !   l.create(n_pt)
!     !   nl.create(n_pt,3)
!     !   .dftgrid.d_r_gill96_exchange_functional(p,np,l,nl)
!     !   local = local + l
!     !   non_local = non_local + nl
!     !   nl.destroy
!     !   l.destroy
!       case default
!         DIE("unknown exchange functional")
!     end
!   end

!   add_r_correlation_matrix(p,local,np,non_local)
!   ! Get terms necessary to calculate the correlation functional matrix.
!   ! Input is p,np.
!   ! Output is local,non_local.
!   ! np and non_local are optional arguments.
!   ! (p means density, np means nabla density).
!   !
!   ! NOTE: When adding a new functional, alter the subroutine
!   !       scfdata.foo/read_dft_correlation also.
!   !
!     p,local :: VEC{REAL}
!     np,non_local :: MAT{REAL}, optional
!     l :: VEC{REAL}*
!     nl :: MAT{REAL}*
!     n_pt :: INT
!     n_pt = size(p)
!     select case (.scfdata.dft_correlation_functional)
!       case ("none") ! Don't add anything.
!       case ("lyp"); .dftgrid.add_r_LYP_c_potential(p,np,local,non_local)
!       case default; DIE("unknown correlation functional")
!     end
!   end

!*******************************************************************************
!  Unrestricted DFT
!*******************************************************************************

   add_XC_matrix(Ka,Kb)
   ! Calculate the exchange correlation matrix numerically.
      Ka,Kb :: MAT{REAL}
      E :: REAL
    ! if (.scfdata.using_GGA_functional) then; .add_GGA_XC_matrix(Ka,Kb)
    ! else;                                    .add_LDA_XC_matrix(Ka,Kb)
    ! end
                                               .add_LDA_XC_matrix(Ka,Kb,E)
      .scfdata.set(dft_energy_correction=E)
   end

   add_LDA_XC_matrix(Ka,Kb,E)
   ! Add the exchange and correlation matrices to K. This routine supports
   ! local functional only, either exchange or correlation.
      Ka,Kb :: MAT{REAL}, target
      E :: REAL
   ENSURE(.basis_info_made, "no basis info")
   ENSURE(.atom.created,  "no atom list")
   ENSURE(.dftgrid.created, "need to specify dftgrid for ZORA")
   ENSURE(.becke_grid.created, "need to specify becke_grid for DFT integrations")
      pt,a0,b0,Vb,Va,W :: MAT{REAL}*
      wt,rho0a,rho0b,V0a,V0b,E0,ab0 :: VEC{REAL}*
      q,a,b,aa,bb,fa,la,na,fb,lb,nb,n_pt :: INT
      atom_a,atom_b,i :: INT
      same_atoms :: BIN
      atom1 :: VEC{INT}(1), target
      atom2 :: VEC{INT}(2), target
      atoms :: VEC{INT}*
      Va_ab,Vb_ab :: REAL
      W.create(.n_bf,.n_bf)
      Va.create(.n_bf,.n_bf)                    ! Use temporary V for parallelisation
      Vb.create(.n_bf,.n_bf)
      nullify(a0); nullify(b0)
      do q = 1,.no_of_atom_pairs
         if (NOT .overlapping_atoms(q)) cycle
         .get_atom_pair_indices(q,atom_a,atom_b,fa,la,na,fb,lb,nb)
         same_atoms = atom_a==atom_b
         if (same_atoms) then; atom1 = [atom_a];        atoms => atom1
         else;                 atom2 = [atom_a,atom_b]; atoms => atom2
         end                                                ! Make the grid points and weights
       ! .becke_grid.make_grid(pt,wt,atoms,compress=FALSE)
       ! .becke_grid.make_grid(pt,wt,atoms,compress=TRUE,weight_is_0=FALSE)
         .becke_grid.make_grid(pt,wt,atoms,compress=TRUE,weight_is_0=TRUE)
         n_pt = pt.dim1                                     ! Keep the weight_is_0 array
         V0a.create(n_pt); V0b.create(n_pt)                 ! local potential grids
         E0.create(n_pt)                                    ! The energy density
         ab0.create(n_pt)             
         .becke_grid.make_bf_grids(a0,b0,pt,atoms)
         rho0a.create(n_pt)                                 ! The density grids
         rho0b.create(n_pt)
         .make_density_grid_u_B(rho0a,rho0b,pt,atoms,a0,b0) ! re-use a0, b0 grids
         V0a = ZERO; V0b = ZERO                    
         .add_u_XC_potentials(V0a,V0b,rho0a,rho0b)          ! Evaluation the XC potentials
         .set_u_XC_energy_density(E0,rho0a,rho0b)
         rho0b.destroy; rho0a.destroy
  ! stdout.show("q =",q)
  ! stdout.text("V0a:")
  ! stdout.put(V0a)
  ! stdout.text("V0b:")
  ! stdout.put(V0a)
  ! stdout.text("E:")
  ! stdout.put(E0)
         V0a = wt*V0a
         V0b = wt*V0b
         E0  = wt*E0
  ! stdout.text("V0a:")
  ! stdout.put(V0a)
  ! stdout.text("V0b:")
  ! stdout.put(V0a)
  ! stdout.text("E:")
  ! stdout.put(E0)
         wt.destroy; pt.destroy
         ! Make the exchange correlation matrix
         do a = 1,na
         do b = 1,nb
            ab0 = a0(:,a)*b0(:,b)
            Va_ab = sum(V0a*ab0) 
            Vb_ab = sum(V0b*ab0) 
            aa = fa + a
            bb = fb + b
            Va(aa,bb) = Va_ab                     
            Vb(aa,bb) = Vb_ab                     
          ! W(aa,bb)  = -HALF*(Va_ab+Vb_ab) + sum(E0*ab0)  ! Make the DFT energy correction matrix
            W(aa,bb)  = sum(E0*ab0)  ! Make the DFT energy correction matrix
  ! stdout.show("Va =",Va_ab)
  ! stdout.show("Vb =",Vb_ab)
  ! stdout.show("e  =",sum(E0*ab0))
         end
         end
  ! stdout.text("Va:")
  ! stdout.put(Va(fa+1:la,fb+1:lb))
  ! stdout.text("Vb:")
  ! stdout.put(Vb(fa+1:la,fb+1:lb))
  ! stdout.text("W:")
  ! stdout.put(W(fa+1:la,fb+1:lb))
         ab0.destroy; E0.destroy; V0b.destroy; V0a.destroy
         if (same_atoms) then
            a0.destroy
            nullify(b0)
         else
            b0.destroy
            a0.destroy
         end
      end
      Ka = Ka + Va
      Kb = Kb + Vb
      Ka.symmetric_reflect
      Kb.symmetric_reflect
      Va = -HALF*Va + W
      Vb = -HALF*Vb + W
      Va.symmetric_reflect
      Vb.symmetric_reflect
      E = .density_matrix.alpha.trace_of_product_with(Va) &
        +  .density_matrix.beta.trace_of_product_with(Vb)
      Vb.destroy; Va.destroy
      W.destroy
   end

