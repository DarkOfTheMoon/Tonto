   r_KS_energy_correction result(res)
   ! The total Kohn-Sham electronic energy can be written as....
   !   E = sum(P*(F+H)) + correction
   ! where the Kohn-Sham energy correction is
   !   correction = -(sum(P*K)) + Exc
   ! where K is the exchange-correlation contribution to the Fock matrix.
     res :: REAL
   ENSURE(.basis_info_made, "no basis info")
   ENSURE(.atom.created,  "no atom list")
   ENSURE(.dftgrid.created, "need to specify dftgrid for ZORA")
   ENSURE(.becke_grid.created, "need to specify becke_grid for DFT integrations")
     pt,np,non_local :: MAT{REAL}*
     wt,p,local,total :: VEC{REAL}*
     n_pt :: INT
     tmp :: REAL
      ! Ensure the NO's exist so we can calculate the density for the XC part
      if (.molecular_orbitals.all_destroyed) then; .make_natural_orbitals !  leaky
      else;                                        .assign_NOs_to_MOs
      end
     ! Get the integration grid
   ! .becke_grid.set_atoms_and_positions(.atom.atomic_number,.atom.transposed_coordinates)
     n_pt = .becke_grid.no_of_points
     wt.create(n_pt); pt.create(n_pt,3)
     .becke_grid.make_grid(pt,wt)
     ! Get the densities and their gradients.
     p.create(n_pt)
     .make_density_grid(p,pt)
     if (.scfdata.using_GGA_functional) then
        np.create(n_pt,3)
        .make_nabla_density_grid(np,pt)
     end
     pt.destroy
     ! Get the local and nonlocal contributions.
     local.create(n_pt)
     local = ZERO
     if (.scfdata.using_GGA_functional) then
        non_local.create(n_pt,3)
        non_local = ZERO
     end
     .add_r_XC_potentials(local,p,non_local,np)
     ! This is -1/2 \int v_xc rho dr.
     total.create(n_pt)
     total = local*p
     if (.scfdata.using_GGA_functional) then
        total = total + sum(non_local(:,:)*np(:,:),dim=2)
        non_local.destroy
     end
     total = -HALF*total
 ! tmp = sum(wt*total)
 ! stdout.show("KS v term =",tmp)
     local.destroy
     ! Now add Exc
     .add_r_XC_functionals(total,p,np)
     res = sum(wt*total)
 ! stdout.show("KS E term =",res-tmp)
 ! stdout.show("KS cxn    =",res)
     wt.destroy
     total.destroy
     np.destroy
     p.destroy
   ! .becke_grid.destroy_atom_info
   end

