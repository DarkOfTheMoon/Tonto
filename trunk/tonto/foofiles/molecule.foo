          jp(k,3) = jp(k,3) - fac*Jzab.trace_product_with(real(aimag(Dba),kind=REAL_KIND))
        end
        Jxab.destroy
        Jyab.destroy
        Jzab.destroy
        sh.destroy_ptr_part
      end
      pt.destroy
      jp.zero_small_values(TOL(10))
