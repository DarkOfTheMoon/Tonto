   set_interpolator_info(plot_kind,interpolator) ::: leaky
   ! Set the interpolator info for all the atoms if the "plot_kind" is such
   ! that interpolation tables could be beneficial. This should be called just
   ! prior to doing the plot to take advantage of speed-ups.
      plot_kind :: STR
      interpolator :: INTERPOLATOR*
   ENSURE(.atom.created,"no atoms")
      if (plot_kind/="stockholder_density" AND plot_kind/="promolecule_density") return
      ! If we get here, the interpolator information will be used
      if (interpolator.created) then ! use interpolator as the default
         if (.slaterbasis.created)  .atom.make_slater_interpolators(interpolator)
         if (.coppensbasis.created) .atom.make_coppens_interpolators(interpolator)
      else ! always make default interpolators for stockholder or promolecule isosurfaces
         if (.slaterbasis.created)  .atom.make_slater_interpolators
         if (.coppensbasis.created) .atom.make_coppens_interpolators
      end
   end

   set_d_interpolator_info(isosurface) ::: leaky
   ! Set the interpolator info for all the atoms if the isosurface kind is such
   ! that interpolation tables could be beneficial. This should be called just
   ! prior to doing the plot to take advantage of speed-ups.
      isosurface :: ISOSURFACE
   ENSURE(.atom.created,"no atoms")
      plot_kind :: STR
      plot_kind = isosurface.iso_kind
      if (plot_kind/="promolecule_density") return
      ! If we get here, the interpolator information will be used
      if (isosurface.interpolator.created) then ! use interpolator as the default
         if (.slaterbasis.created)  .atom.make_slater_d_interpolators(isosurface.interpolator)
       ! if (.coppensbasis.created) .atom.make_coppens_interpolators(isosurface.interpolator)
      else ! always make default interpolators for stockholder or promolecule isosurfaces
         if (.slaterbasis.created)  .atom.make_slater_d_interpolators
       ! if (.coppensbasis.created) .atom.make_coppens_interpolators
      end
   end

