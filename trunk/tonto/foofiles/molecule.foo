   ENSURE(.occupation_numbers.created, "no occupation numbers")
   ENSURE(.occupation_numbers.created("restricted"),"no occupation numbers")
     orb :: VEC{REAL}*
     nabla_orb :: MAT{REAL}*
     occ :: REAL
