!      r_max = .r_max(cutoff)
!      r2_max = r_max*r_max
!      px = .pos(1) 
!      py = .pos(2) 
!      pz = .pos(3) 
!      do n = 1,pt.dim1
!         xx = pt(n,1) - px 
!         yy = pt(n,2) - py
!         zz = pt(n,3) - pz 
!         rr = xx*xx + yy*yy + zz*zz
!         x(n)  = xx
!         y(n)  = yy
!         z(n)  = zz
