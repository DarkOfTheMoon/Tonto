   set_isotropic_ADP
   ! Set the ADP to be isotropic.
      val :: REAL

      ! The averaged trace
      val = THIRD * (.ADP_tensor(1,1)+.ADP_tensor(2,2)+.ADP_tensor(3,3))

      ! Make it isotropic
      .ADP_tensor      = ZERO
      .ADP_tensor(1,1) = val
      .ADP_tensor(2,2) = val
      .ADP_tensor(3,3) = val

   end

