   resolve_basis(basis,suffix,found) ::: template
   ! Resolve the .BASE by firstly trying to matching the ".LABEL" with one of
   ! the labels from "basis". If that fails, and "suffix" is present and not
   ! blank, then a library basis label is generated from the "suffix" and we try
   ! and match again to one of the labels in "basis". If present, "found" is set
   ! TRUE if the basis set has been resolved, or FALSE otherwise.
      basis :: VEC{BASIS}*
      suffix :: STR, optional
      found :: BIN, optional
   ENSURE(basis.created,"no basis")
      library_label :: STR
      fnd :: BIN
      fnd = FALSE
      if (present(suffix)) then
      if (suffix/=" ") then
        library_label = .library_basis_label(suffix)
        BASE.resolve_by_label(library_label,basis,clobber=TRUE,found=fnd)
        if (fnd) LABEL = library_label
      end
      end
      if (NOT fnd AND LABEL/=" ") then
         BASE.resolve_by_label(LABEL,basis,clobber=TRUE,found=fnd)
      end
      if (present(found)) found = fnd
   end

