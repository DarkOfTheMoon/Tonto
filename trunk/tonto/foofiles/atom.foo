!      keep :: VEC{BIN}
!      pt :: MAT{REAL}, IN
!      cutoff :: REAL, IN
!   ENSURE(.basis.created,"no basis set")
!      xx,yy,zz,rr, px,py,pz, r_max,r2_max :: REAL
!      n :: INT
!      r_max = .r_max(cutoff)
!      r2_max = r_max*r_max
!      px = .pos(1) 
!      py = .pos(2) 
!      pz = .pos(3) 
!      do n = 1,pt.dim1
!         xx = pt(n,1) - px 
!         yy = pt(n,2) - py
!         zz = pt(n,3) - pz 
!         rr = xx*xx + yy*yy + zz*zz
!         x(n)  = xx
!         y(n)  = yy
!         z(n)  = zz
!         r2(n) = rr
!         keep(n) = rr<=r2_max
!      end
!   end

!   make_displaced_points(x,y,z,r2,keep,pt,cutoff,keep2)
!   ! From a given list of points "pt" return the displacements "x", "y", "z" and
!   ! their square "r2". This saves work in the grid routines. Also return a list
!   ! which tells those points for which the basis functions of the atom have a
!   ! significant value above "cutoff" i.e. those points for which "keep" is
!   ! FALSE. "keep2" is an additional list of values to keep. NOTE: *all*
!   ! displacement values are returned.
!      x,y,z,r2 :: VEC{REAL}
!      keep :: VEC{BIN}
!      pt :: MAT{REAL}, IN
!      cutoff :: REAL, IN
!      keep2 :: VEC{BIN}, IN
!   ENSURE(.basis.created,"no basis set")
!      xx,yy,zz,rr, px,py,pz, r_max,r2_max :: REAL
