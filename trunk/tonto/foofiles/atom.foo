     laplacian_grid :: VEC{REAL}
     pt :: MAT{REAL}
   ENSURE(pt.dim2==3, "wrong dimension for pt array")
   ENSURE(.natural_orbitals.created("restricted"), "no restricted NO's")
   ENSURE(.occupation_numbers.restricted.created, "no occupation #'s")
     NO :: VEC{REAL}*
     P,D :: MAT{REAL}*
     n_occ,n,n_pt :: INT
     n_pt = pt.dim1
     laplacian_grid = ZERO
     NO.create(n_pt)
     P.create(n_pt,3)
