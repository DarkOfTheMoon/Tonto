   put_ADP2_vector_to(U,fac)
   ! Put the second-order ADPs into vector "U".
   ! If present, include length-conversion factor "fac".
      self :: IN
      U :: VEC{REAL}, OUT
      fac :: REAL, optional, IN

   DIE_IF(U.dim/=6,"wrong size, U")

      fac2 :: REAL

      ! In atomic units
      if (NOT present(fac)) then

         ! Set from ADP tensor
         U(1)     =      .ADP_tensor(1,1)
         U(2)     =      .ADP_tensor(2,2)
