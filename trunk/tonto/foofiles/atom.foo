   make_dft_xc_pot_grid(V,pt,exchange,correlation)
   ! Work out the DFT exchange-correlation potential "V" at a series of points
   ! "pt".
      V :: VEC{REAL}
      pt :: MAT{REAL}
      exchange,correlation :: STR

      dft :: DFT_FUNCTIONAL
      rho :: VEC{REAL}*

      rho.create(V.dim)
      .make_density_grid(rho,pt)
      V = ZERO
