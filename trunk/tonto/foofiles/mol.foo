!      atom_a = .atom_for_shell(i_a)
!      atom_b = .atom_for_shell(i_b)
!      if (.scfdata.nudo AND .atom.not_bonded(atom_a,atom_b)) cycle
!      fa = .first_basis_fn_for_shell(i_a)
!      fb = .first_basis_fn_for_shell(i_b)
!      la = .last_basis_fn_for_shell(i_a)
