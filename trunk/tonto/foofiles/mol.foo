!    .fock_matrix.restricted(:.n_a,.n_a+1:).to_product(ONE/(.scfdata.lambda+1))

    .fock_matrix.plus(constraint)

!    ! Change .fock_matrix back to the AO basis 
!    temp.create( .n_bf, .n_bf)
