     do q=1,.n_shell_pairs
        .get_shell_pair(shell,q,fa,la,fb,lb)
        n_a = shell.a.n_comp; n_b = shell.b.n_comp
        QQxx.create(n_a,n_b); QQyy.create(n_a,n_b); QQzz.create(n_a,n_b)
