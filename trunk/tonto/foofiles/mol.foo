    rho_sz.destroy; rho_sy.destroy; rho_sx.destroy; tau.destroy
    d0_s.create(n_pt)
    factor = 0.6d0*(SIX*PI*PI)**(TWO/THREE)
    d0_s = factor*rho_s**(FIVE/THREE)
    rho_s.destroy
    ELF_grid = ONE/(ONE+(d_s*d_s)/(d0_s*d0_s))
