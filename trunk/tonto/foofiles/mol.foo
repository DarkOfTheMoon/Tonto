  ! Get the electron repulsion integrals on disk. If the integral
  ! file is already there, do nothing.
    ARCHIVE :: eri_archive,ind_archive
    eri_archive.set(.name,"eri_integrals")
    ind_archive.set(.name,"eri_index")
