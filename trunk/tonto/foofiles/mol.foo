!      shellquartet.ab => shellpair(ab)
!      do cd = 1,ab
!        .get_shell_pair_indices(cd,i_c,i_d)  ! c & d shell indices.
!        atom_c = .atom_for_shell(i_c)
!        atom_d = .atom_for_shell(i_d)
!        if (.scfdata.nudo AND .atom.not_bonded(atom_c,atom_d)) cycle
