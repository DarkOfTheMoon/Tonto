   F_weighted_r_factor result (res) ::: pure
   ! weighted r factor for the structure factors
      self :: IN
      res :: REAL
   ENSURE(.have_F_pred,"no predicted structure factors")
   ENSURE(.have_F_sigma,"no experimental structure factor errors")
   ENSURE(.have_F_exp,"no experimental structure factors")
      top,bot,z,b :: REAL
      n :: INT
      top = ZERO
      bot = ZERO
      do n=1,size(self)
       z = self(n).F_z
       b = self(n).F_exp / self(n).F_sigma
       top = top + z*z
       bot = bot + b*b
      end
      res = sqrt(top / bot)
   end

   F2_weighted_r_factor result (res) ::: pure
   ! weighted r factor for the structure factors
      self :: IN
      res :: REAL
   ENSURE(.have_F_pred,"no predicted structure factors")
   ENSURE(.have_F_sigma,"no experimental structure factor errors")
   ENSURE(.have_F_exp,"no experimental structure factors")
      top,bot,z,b :: REAL
      n :: INT
      top = ZERO
      bot = ZERO
      do n=1,size(self)
       z = self(n).F2_z
       b = self(n).F2_exp / self(n).F2_sigma
       top = top + z*z
       bot = bot + b*b
      end
      res = sqrt(top / bot)
   end

   F_val_r_factor(core_reflection) result (res) 
   ! Valence r factor . The set of "core_reflections" must be supplied.
      self :: IN
      core_reflection :: VEC{REFLECTION}, IN
      res :: REAL

   ENSURE(.have_F_pred,"no predicted structure factors")
   ENSURE(.have_F_exp,"no experimental structure factors")

      top,bot,val :: REAL
      xi :: VEC{REAL}*
      phase :: VEC{CPX}*
      k :: INT

      ! Get the scale factor/extinction correction
      ! Make the phases too ...
      xi.create(.dim)
      phase.create(.dim)
      do k = 1,.dim
        val = abs(self(k).F_calc)
        if (val.is_zero) then
           xi(k)    = ZERO
           phase(k) = ZERO
        else
           val = ONE/val
           xi(k)    = self(k).F_pred * val
           phase(k) = self(k).F_calc * val
        end
      end

      ! Make the valence (deformation) R factor
      top = ZERO
      bot = ZERO
      do k = 1,.dim
       val = abs(self(k).F_exp*phase(k) - xi(k)*core_reflection(k).F_calc)
       top = top + val - xi(k)*abs(self(k).F_calc - core_reflection(k).F_calc) 
       bot = bot + val
      end
      res = top / bot

      ! Clean up
      phase.destroy
      xi.destroy

   end

   I_weighted_r_factor result (res) ::: pure
   ! weighted r factor for the intensities
      self :: IN
      res :: REAL
   ENSURE(.have_I_pred,"no predicted structure factors")
   ENSURE(.have_I_sigma,"no experimental structure factor errors")
   ENSURE(.have_I_exp,"no experimental structure factors")
      top,bot,z,b :: REAL
      n :: INT
      top = ZERO
      bot = ZERO
      do n=1,size(self)
       z = self(n).I_z
       b = self(n).I_exp / self(n).I_sigma
       top = top + z*z
       bot = bot + b*b
      end
      res = sqrt(top / bot)
   end

