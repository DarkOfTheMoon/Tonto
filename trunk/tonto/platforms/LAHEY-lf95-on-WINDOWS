#DEFS       = -DFLUSH -DBROKEN_TYPE_INITIALIZATION -DUSE_ERROR_MANAGEMENT -DNOGENERIC
DEFS       = -DFLUSH -DBROKEN_TYPE_INITIALIZATION -DUSE_CALL_STACK_MANAGEMENT -DNOGENERIC
MKLIB      = lib
