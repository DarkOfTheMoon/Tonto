  echo $ac_n "(cached) $ac_c" 1>&6
else
  if test -n "$CMP"; then
  ac_cv_prog_CMP="$CMP" # Let the user override the test.
else
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS=":"
  ac_dummy="$PATH"
  for ac_dir in $ac_dummy; do
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_prog_CMP="cmp"
      break
    fi
  done
  IFS="$ac_save_ifs"
fi
fi
CMP="$ac_cv_prog_CMP"
