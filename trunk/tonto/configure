      end
EOF
rm -f conftest.x conftest.o
csh -c "$FC -O0 $LIBS -o conftest.x conftest.${FSUFFIX} >& /dev/null"
LIBS="$ac_save_LIBS"
if eval "test -x conftest.x"; then
  echo "$ac_t""yes" 1>&6
  havelibs="1"
  LIBS="-lcxml $LIBS"
else
  echo "$ac_t""no" 1>&6
fi
rm -f conftest.x conftest.${FSUFFIX} conftest.o
