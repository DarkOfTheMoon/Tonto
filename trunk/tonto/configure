  rm -f conf.test 
  rm -f a.out
  echo "$ac_t""${compiler}" 1>&6

  
  echo $ac_n "checking "Compiler suffix"""... $ac_c" 1>&6
echo "configure:812: checking "Compiler suffix"" >&5
  FSUFFIX=f90
  test ${compiler} = FUJITSU && FSUFFIX=f90
  test ${compiler} = COMPAQ && FSUFFIX=f90
  test ${compiler} = DEC && FSUFFIX=f90
  test ${compiler} = IBM && FSUFFIX=f90
  test ${compiler} = NAG && FSUFFIX=F90
  test ${compiler} = HP && FSUFFIX=f90
  test ${compiler} = SUN && FSUFFIX=F90
  echo "$ac_t""${FSUFFIX}" 1>&6

  
  echo $ac_n "checking "if Fortran 95 compiler"""... $ac_c" 1>&6
echo "configure:825: checking "if Fortran 95 compiler"" >&5
  
cat > conftest.${FSUFFIX} <<EOF
    program main
      integer :: i
      real, dimension(10) :: a
      forall(i=1:10)
         a(i) = 1.0
      end forall
    end
EOF
rm -f conftest.x conftest.o
csh -c "$FC -O0 -o conftest.x conftest.${FSUFFIX} >& /dev/null"
if eval "test -x conftest.x"; then
   f95_compiler="yes"
else
   f95_compiler="no"
fi

  echo "$ac_t""${f95_compiler}" 1>&6

  
  echo "$ac_t""""" 1>&6
  echo "checking Libraries" 1>&6
echo "configure:849: checking Libraries" >&5
  havelibs="0"
  essl="0"
  
if test ${havelibs} != "1"; then
ac_save_LIBS="$LIBS"
echo $ac_n "checking "for -lcxml"""... $ac_c" 1>&6
echo "configure:856: checking "for -lcxml"" >&5
LIBS="-lcxml $LIBS"
cat > conftest.${FSUFFIX} <<EOF
      program main
        write(*,*) lsame("a","b")
