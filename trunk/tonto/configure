  ${PERL} -e 'open(A,"conf.test"); while (<A>) {if (/Fujitsu/) {exit 0}; exit 1;}'
  if test ${?} = 0; then
    compiler=FUJITSU
  fi
fi

  
if test ${compiler} = UNKNOWN; then
  ${PERL} -e 'open(A,"conf.test"); while (<A>) {if (/Compaq/) {exit 0}; exit 1;}'
  if test ${?} = 0; then
    compiler=COMPAQ
  fi
fi

  
if test ${compiler} = UNKNOWN; then
  ${PERL} -e 'open(A,"conf.test"); while (<A>) {if (/Digital/) {exit 0}; exit 1;}'
  if test ${?} = 0; then
    compiler=DEC
  fi
fi

  
if test ${compiler} = UNKNOWN; then
  ${PERL} -e 'open(A,"conf.test"); while (<A>) {if (/IBM/) {exit 0}; exit 1;}'
  if test ${?} = 0; then
    compiler=IBM
  fi
fi

  
if test ${compiler} = UNKNOWN; then
  ${PERL} -e 'open(A,"conf.test"); while (<A>) {if (/NAG/) {exit 0}; exit 1;}'
  if test ${?} = 0; then
    compiler=NAG
  fi
