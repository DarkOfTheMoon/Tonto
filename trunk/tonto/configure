      program main
        write(*,*) lsame("a","b")
        call dgesv(4,nrhs,LU,4,pivot,solution,4,err)
      end
EOF
rm -f conftest.x conftest.o
csh -c "$FC -O0 $LIBS -o conftest.x conftest.${FSUFFIX} >& /dev/null"
