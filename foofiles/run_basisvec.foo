program run_basisvec

   use TYPES
   use SYSTEM
   use TEXTFILE
   use BASISVEC

#include "macros"

   implicit none

   TEXTFILE, PTR :: in,out
   BASISVEC, PTR :: b

   in.create("input")
   in.open_for_read
   stdin => in

   out.create("output")
   out.open_for_write
   stdout => out

   b.read
   b.put

end
