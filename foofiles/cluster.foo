   make_susceptibility2(uc_alpha,chi2_naive,chi2_centroid,chi2_nonH,chi2_nc,chi2_do,chi2)
   ! Make the second ortder susceptibility from distributed
   ! polarisabilities.
      uc_alpha,chi2_naive,chi2_centroid,chi2_nonH,chi2_nc,chi2_do,chi2 :: MAT{REAL}(3,3)

   ENSURE(.crystal.created,"no crystal")
   ENSURE(.asymmetric_cell_atom.created,"no asymetric cell atoms")

      L0,L1,Lm,D,Dnc,Ddo,Dd,AA :: MAT{REAL}* 
      pos,pos0,coc,coc0,dipole,alpha_q,centroid :: MAT{REAL}*
      mol,mol0,ind,ind0,par,non_H :: VEC{INT}*
      charge,r,Lq :: VEC{REAL}*
      alpha_m,alpha :: MAT3{REAL}*
      n,n_m,n_a,n_H,nH,a,b,i,j,m :: INT
      fac :: REAL

      ! Get the electrical properties for each atom
      .get_LFF_molecule_info(mol,ind,par,pos,coc,alpha_q,alpha_m) 

      ! Get no of atoms in unit cell
      n = ind.dim

      ! Make the charge and dipole Lorentz factor tensors
      D.create(3*n,3*n)
      Dnc.create(3*n,3*n)
      Ddo.create(3*n,3*n)
      alpha.create(3,3,n)
      L0.create(3*n,n)
      L1.create(3*n,3*n)
      .crystal.unit_cell.make_LFF_tensors(L0,L1,pos,ind,mol)

      ! Get the atomic alpha's
      alpha = ZERO
      AA.create(3,3)
      r.create(3)
      do a = 1,n
         r = pos(:,a) - coc(:,a)
         AA = r.outer_product_with(alpha_q(:,a)) + alpha_m(:,:,a)
         alpha(:,:,a) = alpha(:,:,a) + AA
      end
      r.destroy; AA.destroy

      ! Make the D matrix
      D.to_unit_matrix
      Dnc.to_unit_matrix
      Ddo.to_unit_matrix
      fac = FOUR*PI/.crystal.unit_cell.volume
      i = 0
      do a = 1,n
         j = 0
         do b = 1,n
            Lq => L0(i+1:i+3,b)      
            Lm => L1(i+1:i+3,j+1:j+3)      

            Dd =>   D(i+1:i+3,j+1:j+3)      
            Dd = Dd - fac*Lq.outer_product_with(alpha_q(:,b))
            Dd = Dd - fac*matmul(Lm,alpha_m(:,:,b))

            Dd => Dnc(i+1:i+3,j+1:j+3)      
            Dd = Dd - fac*matmul(Lm,alpha_m(:,:,b))

            Dd => Ddo(i+1:i+3,j+1:j+3)      
            Dd = Dd - fac*matmul(Lm,alpha(:,:,b))
            
            j = j + 3
         end
         i = i + 3
      end
      D.to_inverse_of(D)
      Dnc.to_inverse_of(Dnc)
      Ddo.to_inverse_of(Ddo)
      L1.destroy
      L0.destroy

      ! Make the distributed polarisanbility chi2 
      chi2 = ZERO
      chi2_nc = ZERO
      chi2_do = ZERO
      uc_alpha = ZERO
      AA.create(3,3)
      r.create(3)
      i = 0
      do a = 1,n
         r = pos(:,a) - coc(:,a)
         AA = r.outer_product_with(alpha_q(:,a)) + alpha_m(:,:,a)
         uc_alpha = uc_alpha + AA
         AA = fac*AA
         j = 0
         do b = 1,n
            Dd =>   D(i+1:i+3,j+1:j+3)      
            chi2 = chi2 + matmul(AA,Dd)
            Dd => Dnc(i+1:i+3,j+1:j+3)      
            chi2_nc = chi2_nc + matmul(AA,Dd)
            Dd => Ddo(i+1:i+3,j+1:j+3)      
            chi2_do = chi2_do + matmul(AA,Dd)
            j = j + 3
         end
         i = i + 3
      end

      ! Clean up
      r.destroy; AA.destroy
      alpha.destroy
      Ddo.destroy; Dnc.destroy; D.destroy

      ! ************************************************

      ! Make chi2_naive
      D.create(3,3)
      D.to_unit_matrix
      D = D - fac*THIRD*uc_alpha
      D.to_inverse_of(D)
      chi2_naive = fac*matmul(uc_alpha,D)
      D.destroy

      ! ************************************************

      ! Get the no of molecules, no of atoms per molecule
      n_m = maxval(mol) 
      DIE_IF(mod(n,n_m)/=0,"no of atoms in a molecule is not integral")
      n_a = n/n_m

      ! Get the molecular alpha's
      alpha.create(3,3,n_m)
      alpha = ZERO
      AA.create(3,3)
      r.create(3)
      a = 0
      do m = 1,n_m
         do i = 1,n_a
            a = a + 1
            r = pos(:,a) - coc(:,a)
            AA = r.outer_product_with(alpha_q(:,a)) + alpha_m(:,:,a)
            alpha(:,:,m) = alpha(:,:,m) + AA
         end
      end
      r.destroy; AA.destroy

      ! Get the centroids, non H positions.
      coc0.create(3,n_m)
      ind0.create(n_m)
      mol0.create(n_m)
      coc0 = coc(:,1:(n_m-1)*n_a+1:n_a)
      ind0 = 1
      mol0 = mol(1:(n_m-1)*n_a+1:n_a)

      ! Print out centroid info ... important
      stdout.flush
      stdout.show("Center of charge =",coc0(:,1))

      ! Make the centroid charge and dipole Lorentz factor tensors
      D.create(3*n_m,3*n_m)
      L0.create(3*n_m,n_m)
      L1.create(3*n_m,3*n_m)
      .crystal.unit_cell.make_LFF_tensors(L0,L1,coc0,ind0,mol0)

      ! Print out centroid info ... important
      stdout.flush
      stdout.text("Arrays for centroid calculation:")
      stdout.text("coc0:")
      stdout.put(transpose(coc0))
      stdout.text("ind0:")
      stdout.put(ind0,"column")
      stdout.text("mol0:")
      stdout.put(mol0,"column")
      stdout.text("Dipole L1 tensor:")
      stdout.put(L1)
      stdout.text("Alpha for each molecule:")
      stdout.put(alpha,as_matrices=TRUE)

      ! Make the D matrix
      D.to_unit_matrix
      fac = FOUR*PI/.crystal.unit_cell.volume
      i = 0
      do a = 1,n_m
         j = 0
         do b = 1,n_m
            Dd => D(i+1:i+3,j+1:j+3)      
            Lm => L1(i+1:i+3,j+1:j+3)      
            Dd = Dd - fac*matmul(Lm,alpha(:,:,b))
            j = j + 3
         end
         i = i + 3
      end
      D.to_inverse_of(D)
      L1.destroy
      L0.destroy

      ! Make the chi2_centroid susceptibility
      chi2_centroid = ZERO
      AA.create(3,3)
      r.create(3)
      i = 0
      do a = 1,n_m
         AA = alpha(:,:,a)
         AA = fac*AA
         j = 0
         do b = 1,n_m
            Dd => D(i+1:i+3,j+1:j+3)      
            chi2_centroid = chi2_centroid + matmul(AA,Dd)
            j = j + 3
         end
         i = i + 3
      end

      ! Clean up
      r.destroy; AA.destroy
      D.destroy
      mol0.destroy; ind0.destroy; coc0.destroy

      ! ************************************************

      ! Get non H positions.
      n_H = count(.asymmetric_cell_atom(par).atomic_number>1)
      non_H.create(n_H)
      non_H = pack([(a,a=1,n)],.asymmetric_cell_atom(par).atomic_number>1)

      ! Get the non H positions, indices, molecules.
      pos0.create(3,n_H)
      ind0.create(n_H)
      mol0.create(n_H)
      pos0 = pos(:,non_H)
      ind0 = ind(non_H)
      mol0 = mol(non_H)

      ! Make the centroid charge and dipole Lorentz factor tensors
      D.create(3*n_H,3*n_H)
      L0.create(3*n_H,n_H)
      L1.create(3*n_H,3*n_H)
      .crystal.unit_cell.make_LFF_tensors(L0,L1,pos0,ind0,mol0)

      ! Print out centroid info ... important
      stdout.flush
      stdout.text("Arrays for non H calculation:")
      stdout.text("pos0:")
      stdout.put(transpose(pos0))
      stdout.text("ind0:")
      stdout.put(ind0,"column")
      stdout.text("mol0:")
      stdout.put(mol0,"column")
      stdout.text("Dipole L1 tensor:")
      stdout.put(L1)
    
      ! Average alpha over non-H sites
      DIE_IF(mod(n_H,n_m)/=0,"no of non-Hydrogen atoms wrong")
      nH = n_H/n_m
      alpha = alpha/nH

      ! Make the D matrix
      D.to_unit_matrix
      fac = FOUR*PI/.crystal.unit_cell.volume
      i = 0
      do a = 1,n_H
         j = 0
         do b = 1,n_H
            Dd => D(i+1:i+3,j+1:j+3)      
            Lm => L1(i+1:i+3,j+1:j+3)      
            m  = (b-1)/nH + 1
            Dd = Dd - fac*matmul(Lm,alpha(:,:,m))
            j = j + 3
         end
         i = i + 3
      end
      D.to_inverse_of(D)
      L1.destroy
      L0.destroy

      ! Make the chi2_nonH susceptibility
      chi2_nonH = ZERO
      AA.create(3,3)
      r.create(3)
      i = 0
      do a = 1,n_H
         m  = (a-1)/nH + 1
         AA = alpha(:,:,m)
         AA = fac*AA
         j = 0
         do b = 1,n_H
            Dd => D(i+1:i+3,j+1:j+3)      
            chi2_nonH = chi2_nonH + matmul(AA,Dd)
            j = j + 3
         end
         i = i + 3
      end

      ! Clean up
      r.destroy; AA.destroy
      D.destroy
      mol0.destroy; ind0.destroy; pos0.destroy
      non_H.destroy
      alpha.destroy
      alpha_m.destroy; alpha_q.destroy
      coc.destroy; pos.destroy
      par.destroy; ind.destroy; mol.destroy

   end

   get_LFF_molecule_info(mol,ind,par,pos,coc,alpha_q,alpha_m) ::: leaky
   ! Get the unit cell molecule information for a distributed local field factor
   ! (LFF) calculation. "coc" are the molecular center of charges.
      mol,ind,par :: VEC{INT}*
      pos,coc,alpha_q :: MAT{REAL}*
      alpha_m :: MAT3{REAL}*

   ENSURE(.crystal.created,"no crystal")
   ENSURE(.unit_cell_mol_for_atom.created,"no unit_cell_mol_for_atom")

      n_atom,n_mol,n,m,u,a :: INT
      mol0,ind0,par0 :: VEC{INT}*
      charge,charge0 :: VEC{REAL}*
      e_dipole,r :: VEC{REAL}(3)
      pos0,dipole,dipole0,alpha_q0 :: MAT{REAL}*
      alpha_m0 :: MAT3{REAL}*

      ! Create the info arrays
      n_mol = .n_unit_cell_mols
      n_atom = .crystal.n_unit_cell_atoms
      mol.create(n_atom)
      ind.create(n_atom)
      par.create(n_atom)
      pos.create(3,n_atom)
      coc.create(3,n_atom)
      alpha_q.create(3,n_atom)
      alpha_m.create(3,3,n_atom)
      charge.create(n_atom)
      dipole.create(3,n_atom)

      ! Set n_atom to the # of atoms in one molecule
      DIE_IF(mod(n_atom,n_mol)/=0,"# of unit cell atoms is not divisible by number of molecules")
      n_atom = n_atom/n_mol

      n = 0
      do m = 1,n_mol

         ! Find atom "u" on molecule "m", defragment, get the info
         do u = 1,.crystal.n_unit_cell_atoms

            if (.unit_cell_mol_for_atom(u)/=m) cycle

            mol(n+1:n+n_atom) = m
            ind0     => ind(n+1:n+n_atom)
            par0     => par(n+1:n+n_atom)
            pos0     => pos(:,n+1:n+n_atom)
            charge0  => charge(n+1:n+n_atom)
            dipole0  => dipole(:,n+1:n+n_atom)
            alpha_q0 => alpha_q(:,n+1:n+n_atom)
            alpha_m0 => alpha_m(:,:,n+1:n+n_atom)

            .do_defragment_uc_atom(u,ind0,par0,pos0,charge0,dipole0,alpha_q0,alpha_m0)

            exit

         end

         ! Calculate electronic dipole and centre of charge
         e_dipole = ZERO
         do a = 1,n_atom
            e_dipole = e_dipole + dipole0(:,a)
            e_dipole = e_dipole + charge0(a)*pos0(:,a)
         end
         e_dipole = e_dipole/sum(charge0)
         coc(:,n+1:n+n_atom) = spread(e_dipole,2,n_atom)

         n = n + n_atom

      end

         ! Debug
       ! stdout.flush
       ! stdout.text("mol:")
       ! stdout.put(mol,"column")
       ! stdout.text("ind:")
       ! stdout.put(ind,"column")
       ! stdout.text("pos:")
