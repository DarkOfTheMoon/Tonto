      a :: A_TYPE, IN
      v :: V_TYPE, IN
      transpose_a :: BIN, optional
   end

   minus_product_of(a,v,transpose_a) ::: get_from(VEC{INTRINSIC}, A_TYPE=>MAT{REAL}, V_TYPE=>VEC{REAL})
