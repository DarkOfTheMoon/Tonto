program run_shellvec

   use TYPES
   use SYSTEM
   use SHELL
   use SHELLVEC
   use TEXTFILE

#include "macros"

   implicit none

   TEXTFILE, PTR :: in,out
   SHELLVEC, PTR :: sh

   in.create("input")
   in.open_for_read
   stdin => in

   out.create("output")
   out.open_for_write
   stdout => out

   sh.read
   sh.put

end
