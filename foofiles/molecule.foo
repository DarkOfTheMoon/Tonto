      q,fa,la,na,fb,lb,nb,atom_a,atom_b :: INT
      sh :: SHELL2

      do q = 1,.n_shell_pairs

         .get_precomputed_shell_pair(sh,q,fa,la,na,fb,lb,nb,atom_a,atom_b)

         if (.atom.bonded(atom_a,atom_b)) then; sh.get_kei(T(fa:la,fb:lb))
