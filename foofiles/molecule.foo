   make_nuclear_matrix(Z)
   ! Calculate the nuclear attraction matrix "Z".
      Z :: MAT{REAL}

   ENSURE(.basis_info_made, "no basis info")
   ENSURE(.atom.created,  "no atom list")

      atom :: ATOM*
      Z_c :: MAT{REAL}*
      q,c,fa,la,na,fb,lb,nb :: INT
      sh :: SHELL2

      Z = ZERO

      parallel do q = 1,.n_shell_pairs

       .get_precomputed_shell_pair(sh,q,fa,la,na,fb,lb,nb)

       Z_c.create(na,nb)
       do c = 1,.n_atom
          atom => .atom(c)
          sh.get_nuc(Z_c, atom.mass, atom.pos)
          Z(fa:la,fb:lb) = Z(fa:la,fb:lb) - atom.atomic_number * Z_c
       end

       Z_c.destroy
       sh.destroy_ptr_part

      end

      if (DO_IN_PARALLEL) then
        PARALLEL_SYMMETRIC_SUM(Z)
      else
        Z.symmetric_reflect
      end

      .put_debug(Z,"make_nuclear_matrix: Z")

   end

