! Hirshfeld atom info

   put_Hirshfeld_atom_info ::: leaky
   ! Put the gaussian charges
   ENSURE(.basis_info_made,"no basis info")
   ENSURE(.atom.created,"no atom list")
      e_dipole,n_dipole,t_dipole :: VEC{REAL}(3)
      ed,nd,td,dp :: REAL
      a :: INT

      a = 1

      ! Make and set the atom properties first ...
      if      (.debugging("make_Hirshfeld_atom_info: use old")) then
         .make_Hirshfeld_atom_info_old(output=TRUE)
      else if (.debugging("make_Hirshfeld_atom_info: use orig")) then
         .make_Hirshfeld_atom_info_orig(output=TRUE)
      else
         .make_Hirshfeld_atom_info(output=TRUE)
      end

      ! Calculate dipole contributions
      e_dipole = ZERO
      n_dipole = ZERO
      do a = 1,.n_atom
         e_dipole = e_dipole + .atom(a).dipole
         n_dipole = n_dipole + .atom(a).charge*.atom(a).pos
      end
      t_dipole = e_dipole + n_dipole
      ed = e_dipole.norm
      nd = n_dipole.norm
      td = t_dipole.norm

      ! Write results in au
      stdout.flush
      stdout.text("Hirshfeld atom charges and Hirshfeld atom dipole moments (au):")
      stdout.flush
      stdout.dash(int_fields=1,real_fields=5)
