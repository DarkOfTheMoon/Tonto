     c = SPEED_OF_LIGHT_AU
     alpha = ONE/c
     alpha2= alpha*alpha
 
     E.create(n_gcm)
     p.create(n_gcm)
     ep.create(n_gcm)
     Ap.create(n_gcm)
     bp.create(n_gcm)

     E=ZERO 
     p=ZERO
     ep=ZERO
     Ap=ZERO
     bp=ZERO

     do i = 1,n_gcm

       E(i)= c * sqrt(P2(i) + c * c)
       p(i)=sqrt(P2(i))
       ep(i)=sqrt(ONE+P2(i)*alpha2) 
       Ap(i)= sqrt( (E(i) + c * c)/(TWO * E(i)) )  
       bp(i)=ONE/(ep(i)+ONE)

     end  
    P2.destroy

     do i=1,n_gcm
       do j=1,n_gcm
    
        Z(i,j) = Ap(i)*Z(i,j)*Ap(j)
