   ket :: VEC{MAT_{REAL}}*
   ! The array of contracted wavefunctions in the ket

   special_bra :: VEC{VEC_{INT}}*
   ! This bra contains only the indices of one geminal product basis function.

   special_ket :: VEC{VEC_{INT}}*
