   type SHELL1

   l :: INT  DEFAULT(0)
   ! l quantum number of the shell

   n_comp :: INT  DEFAULT(0)
   ! No. of components for the shell

   n_cc :: INT  DEFAULT(0)
   ! No. of contraction coefficients for the shell

   pos :: VEC{REAL}(3)  DEFAULT(ZERO)
