   end

   array type VEC{PERIODIC_FMM_LEVEL}

   ! A 1-D array of PERIODIC_FMM_LEVEL objects

   end

   type GEMINAL_WAVEFUNCTION

   n_geminals :: INT
   ! The number of geminal functions

   n_bf :: INT
   ! The number of one-particle basis functions

   bra :: VEC{MAT_{REAL}}*
