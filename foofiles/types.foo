   end

   type GEMINAL_MF_SPECTRUM

   n_geminals :: INT
   ! The number of geminal functions

   n_bf :: INT
   ! The number of one-particle basis functions

   n_bas :: INT
   ! The number of basis geminal functions for the particular contraction
   ! considered

   bra :: VEC{MAT_{REAL}}*
