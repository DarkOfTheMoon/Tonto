   put_cif
   ! Output the atoms in CIF format to "stdout"

      i :: INT
      label :: STR

      ! Save CIF settings for later
      stdout.save
      stdout.set_real_precision(4)
      stdout.set_real_width(8)

      ! CIF coordinate header
      stdout.text("loop_")
      stdout.text("_atom_site_label")
      stdout.text("_atom_site_fract_x")
      stdout.text("_atom_site_fract_y")
      stdout.text("_atom_site_fract_z")
      stdout.text("_atom_site_adp_type")
      stdout.text("_atom_site_occupancy")
      
      ! Loop over atoms, put coordinates
      do i = 1,.dim
         stdout.put(self(i).label.trim,width=len_trim(self(i).label))
         stdout.put(self(i).pos(1))
         stdout.put(self(i).pos(2))
         stdout.put(self(i).pos(3))
         stdout.put("Uani")
         stdout.put(self(i).site_occupancy)
         stdout.buffer.string.replace_all("  "," ",repeatedly=TRUE)
         stdout.flush
      end

      stdout.flush 
      stdout.flush 

      ! CIF ADP header
      stdout.text("loop_")
      stdout.text("_atom_site_aniso_label")
      stdout.text("_atom_site_aniso_U_11")
      stdout.text("_atom_site_aniso_U_22")
      stdout.text("_atom_site_aniso_U_33")
      stdout.text("_atom_site_aniso_U_12")
      stdout.text("_atom_site_aniso_U_13")
      stdout.text("_atom_site_aniso_U_23")

      ! Put ADP's
      do i = 1,.dim
         ! Order for the ADP's .......
         ! u11 u22 u33 u12 u13 u23 XD-format
         ! u11 u22 u33 u12 u23 u13 SHELX
         stdout.put(self(i).label.trim,width=len_trim(self(i).label))
         stdout.put(self(i).thermal_tensor(1,1).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(2,2).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(3,3).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(1,2).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(1,3).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(2,3).to_units("angstrom^2"))
         stdout.buffer.string.replace_all("  "," ",repeatedly=TRUE)
         stdout.flush 
      end

      stdout.unsave

   end

   put_cif_with_errors(delta_p)
   ! Output the atoms in CIF format to "stdout"
   ! WARNING: Errors "delta_p" must be given in the crystal axis system.
      delta_p :: VEC{REAL}

      i,k :: INT

      ! Reset precision for errors
      stdout.save
      stdout.set_real_width(stdout.saved.real_width+3)
      stdout.set_real_precision(stdout.saved.real_precision+3)

      ! CIF coordinate header
      stdout.flush
      stdout.text("loop_")
      stdout.text("_atom_site_label")
      stdout.text("_atom_site_fract_x")
      stdout.text("_atom_site_fract_y")
      stdout.text("_atom_site_fract_z")
      stdout.text("_atom_site_adp_type")
      stdout.text("_atom_site_occupancy")

      ! Loop over atoms
      k = 0
      do i = 1,.dim
         stdout.put(self(i).label.trim,width=len_trim(self(i).label))
         stdout.put(self(i).pos(1),delta_p(k+1))
         stdout.put(self(i).pos(2),delta_p(k+2))
         stdout.put(self(i).pos(3),delta_p(k+3))
         stdout.put("Uani")
         stdout.put(self(i).site_occupancy)
         stdout.flush         
         k = k + 9
      end

      ! CIF ADP header
      stdout.flush
      stdout.text("loop_")
      stdout.text("_atom_site_aniso_label")
      stdout.text("_atom_site_aniso_U_11")
      stdout.text("_atom_site_aniso_U_22")
      stdout.text("_atom_site_aniso_U_33")
      stdout.text("_atom_site_aniso_U_12")
      stdout.text("_atom_site_aniso_U_13")
      stdout.text("_atom_site_aniso_U_23")

      ! Put ADP's
      k = 0
      do i = 1,.dim
         ! Order for the ADP's .......
         ! u11 u22 u33 u12 u13 u23 XD-format
         ! u11 u22 u33 u12 u23 u13 SHELX
         stdout.put(self(i).label.trim,width=len_trim(self(i).label))

         ! Put quantity q with error e in format q(e)
         stdout.put(self(i).thermal_tensor(1,1),delta_p(k+4),units="angstrom^2")
         stdout.put(self(i).thermal_tensor(2,2),delta_p(k+5),units="angstrom^2")
         stdout.put(self(i).thermal_tensor(3,3),delta_p(k+6),units="angstrom^2")
         stdout.put(self(i).thermal_tensor(1,2),delta_p(k+7),units="angstrom^2")
         stdout.put(self(i).thermal_tensor(1,3),delta_p(k+8),units="angstrom^2")
         stdout.put(self(i).thermal_tensor(2,3),delta_p(k+9),units="angstrom^2")
         stdout.flush         

         k = k + 9

      end

      stdout.unsave

   end

