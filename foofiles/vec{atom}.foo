         end
         if (coppens) then
            if (label/=" ") then; label = trim(label)//", "//trim(self(i).coppensbasis.label)
            else;                 label = self(i).coppensbasis.label
            end
         end
         stdout.put(label)
         stdout.flush
      end
      stdout.dash(int_fields=3,real_fields=4)
   end

