         self(a).pos = self(a).pos*BOHR_PER_ANGSTROM
      end
   end

   convert_to_angstrom
   ! Convert atom positions to Angstroms from A.U.
       a :: INT
      do a = 1,.dim
         self(a).pos = self(a).pos*ANGSTROM_PER_BOHR
      end
