      a :: INT
      has = TRUE
      do a = 1,.dim
         has = self(a).interpolator.created
         if (NOT has) exit
      end
   end

   has_all_slater_interpolators result(has)
   ! Return TRUE if all atoms have slaterbases which have interpolators.
      self :: IN
      has :: BIN
      a :: INT
      has = TRUE
      do a = 1,.dim
         has = self(a).slaterbasis.created
