   put_NKA_with_errors(delta_p,T_sequence_no,fractional_coords)
   ! Output the atom positions, ADP's, ADP sigmas in NKA file format
   ! to "stdout". Atom positions and ADP's are in fractional_coords
   ! only if requested. Positional sigmas are NOT output.
      delta_p :: VEC{REAL}
      T_sequence_no :: INT
      fractional_coords :: BIN, optional

      i,k :: INT
      crystal :: BIN

      crystal = FALSE
      if (present(fractional_coords)) crystal = fractional_coords

      ! Coordinates and ADP's in A^2
      stdout.flush
      do i = 1,.dim

         stdout.put(self(i).chemical_symbol,width=3)

         ! Coordinates in crystal or cartesian system
         if (crystal) then
            stdout.put(self(i).pos(1))
            stdout.put(self(i).pos(2))
            stdout.put(self(i).pos(3))
         else
            stdout.put(self(i).pos(1).to_units("angstrom"))
            stdout.put(self(i).pos(2).to_units("angstrom"))
            stdout.put(self(i).pos(3).to_units("angstrom"))
         end
         stdout.put(1,width=2)
         stdout.put(T_sequence_no,width=3)
         stdout.flush

         ! ADP's always in cartesians and A^2
         stdout.put(self(i).thermal_tensor(1,1).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(2,2).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(3,3).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(1,2).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(1,3).to_units("angstrom^2"))
         stdout.put(self(i).thermal_tensor(2,3).to_units("angstrom^2"))
         stdout.flush

      end

      ! Now the ADP errors
      stdout.flush
      k = -9
      do i = 1,.dim

         k = k + 9

         stdout.put("SIG",width=3)
         stdout.put(self(i).chemical_symbol,width=3)
         stdout.put(T_sequence_no,width=3)
         stdout.put(delta_p(k+4).to_units("angstrom^2"))
         stdout.put(delta_p(k+5).to_units("angstrom^2"))
         stdout.put(delta_p(k+6).to_units("angstrom^2"))
         stdout.put(delta_p(k+7).to_units("angstrom^2"))
         stdout.put(delta_p(k+8).to_units("angstrom^2"))
         stdout.put(delta_p(k+9).to_units("angstrom^2"))
         stdout.flush

      end

   end

   put_excel_with_errors(delta_p,temperature)
   ! Output the atoms in Excel file format to "stdout"
   ! This should be in the crystal coordinate system.
      delta_p :: VEC{REAL}
      temperature :: REAL

      i,k :: INT

      ! Coordinates in crystal axis system
      stdout.show("Coordinates refined at T = ",temperature)
      stdout.flush
      k = 0
      do i = 1,.dim

         stdout.put("x")
         stdout.put(self(i).pos(1))
         stdout.put(delta_p(k+1))
         stdout.put(self(i).label)
         stdout.flush

         stdout.put("y")
         stdout.put(self(i).pos(2))
         stdout.put(delta_p(k+2))
         stdout.put(self(i).label)
         stdout.flush

         stdout.put("z")
         stdout.put(self(i).pos(3))
         stdout.put(delta_p(k+3))
         stdout.put(self(i).label)
         stdout.flush

         k = k + 9

      end

      ! ADP's in crystal (CIF) axis system, angstrom^2 units
      stdout.show("ADP's refined at T = ",temperature)
      stdout.flush
      k = 0
      do i = 1,.dim

         stdout.put("U11")
         stdout.put(self(i).thermal_tensor(1,1).to_units("angstrom^2"))
         stdout.put(delta_p(k+4).to_units("angstrom^2"))
         stdout.put(self(i).label)
         stdout.flush

         stdout.put("U22")
         stdout.put(self(i).thermal_tensor(2,2).to_units("angstrom^2"))
         stdout.put(delta_p(k+5).to_units("angstrom^2"))
         stdout.put(self(i).label)
         stdout.flush

         stdout.put("U33")
         stdout.put(self(i).thermal_tensor(3,3).to_units("angstrom^2"))
         stdout.put(delta_p(k+6).to_units("angstrom^2"))
         stdout.put(self(i).label)
         stdout.flush

         stdout.put("U12")
         stdout.put(self(i).thermal_tensor(1,2).to_units("angstrom^2"))
         stdout.put(delta_p(k+7).to_units("angstrom^2"))
         stdout.put(self(i).label)
         stdout.flush

         stdout.put("U13")
         stdout.put(self(i).thermal_tensor(1,3).to_units("angstrom^2"))
         stdout.put(delta_p(k+8).to_units("angstrom^2"))
         stdout.put(self(i).label)
         stdout.flush

         stdout.put("U23")
         stdout.put(self(i).thermal_tensor(2,3).to_units("angstrom^2"))
         stdout.put(delta_p(k+9).to_units("angstrom^2"))
         stdout.put(self(i).label)
         stdout.flush

         k = k + 9

      end

   end

   put_bond_length_table(covariance_matrix,all) 
   ! Output the bond length table with standard errors given a
   ! "covariance_matrix" for the atom positions. If "all" is present
   ! and true, put out all possible bond lengths
      covariance_matrix :: MAT{REAL}, IN
      all :: BIN, IN, optional

   ENSURE(covariance_matrix.dim1==3*.dim,"wrong size")

      print_all_bonds :: BIN
      atom_kind :: VEC{VEC_{INT}}*
      n_k,k,l,kk,ll,a,b :: INT
      symbol_a,symbol_b :: STR
      r_ab,err :: REAL
      der :: VEC{REAL}(6)
      cov :: MAT{REAL}(6,6)

      print_all_bonds = FALSE
      if (present(all)) print_all_bonds = all

      if (.dim<2) return
      if (.no_of_bonds<1 AND NOT print_all_bonds) return

      ! Increase accuracy
      stdout.save
      stdout.set_real_width(stdout.saved.real_width+3)
      stdout.set_real_precision(stdout.saved.real_precision+3)

      ! Get atom kinds
      .make_atom_kind_list(atom_kind)
      n_k = size(atom_kind)

      ! Table header
      stdout.flush
      stdout.text("Bond lengths:",flush=2)
      stdout.show("No. of independent bonds  =",.no_of_bonds)
      stdout.flush
      stdout.dash(int_fields=2,real_fields=3)
      stdout.put("Atom_a",int_width=TRUE)
      stdout.put("Atom_b",int_width=TRUE)
      stdout.put("r_ab/Angstrom")
      stdout.put("r_ab/Angstrom")
      stdout.put("err/Angstrom")
      stdout.flush
      stdout.dash(int_fields=2,real_fields=3)
      
      ! Table
      do k = 1,n_k
      do kk = 1,size(atom_kind(k).element)

         a = atom_kind(k).element(kk)
         symbol_a = .list_symbol(a)

         do l = 1,n_k
         do ll = 1,size(atom_kind(l).element)

            b = atom_kind(l).element(ll)
            symbol_b = .list_symbol(b)

            if (a>=b) cycle
            if (NOT print_all_bonds AND NOT .bonded(a,b)) cycle

            ! Distance and errors
            r_ab = .bond_distance(a,b)
            r_ab.convert_to("angstrom")

            ! Extract covariances for atoms a, b
            covariance_matrix.put_blocks_to(cov,block_dim=3,block_list=[a,b])

            ! Make error
            .bond_distance_deriv(a,b,der)
            err = dot_product(der,matmul(cov,der))
            err = sqrt(err)
            err.convert_to("angstrom")

            ! Print out
            stdout.put(symbol_a,int_width=TRUE)
            stdout.put(symbol_b,int_width=TRUE)
            stdout.put(r_ab,err)
            stdout.put(r_ab)
            stdout.put(err)
            stdout.flush

         end
         end

      end
      end
      stdout.dash(int_fields=2,real_fields=3)

      ! Clean & unsave
      atom_kind.destroy
      stdout.unsave

   end

   put_bond_angle_table(covariance_matrix,all)
   ! Output the bond length table with standard errors given a
   ! "covariance_matrix" for the atom positions. If "all" is present
   ! and true, put out all possible angles
      covariance_matrix :: MAT{REAL}, IN
      all :: BIN, IN, optional

      print_all_angles,abc_colinear :: BIN
      atom_kind :: VEC{VEC_{INT}}*
      n_k,k,l,m,kk,ll,mm,a,b,c :: INT
      symbol_a,symbol_b,symbol_c :: STR
      theta_abc,err :: REAL
      der :: VEC{REAL}(9)
      cov :: MAT{REAL}(9,9)

      print_all_angles = FALSE
      if (present(all)) print_all_angles = all

      if (.dim<3 ) return
      if (.no_of_angles<1 AND NOT print_all_angles) return
      if (.no_of_angles>100 AND NOT print_all_angles) return

      ! Increase accuracy
      stdout.save
      stdout.set_real_width(stdout.saved.real_width+3)
      stdout.set_real_precision(stdout.saved.real_precision+3)

      ! Get atom kinds
      .make_atom_kind_list(atom_kind)
      n_k = size(atom_kind)

      ! Table header
      stdout.flush
      stdout.text("Bond angles (b the central atom):",flush=2)
      stdout.show("No. of independent angles =",.no_of_angles)
      stdout.flush
      stdout.dash(int_fields=3,real_fields=3)
      stdout.put("Atom_a",int_width=TRUE)
      stdout.put("Atom_b",int_width=TRUE)
      stdout.put("Atom_c",int_width=TRUE)
      stdout.put("Angle_abc/degrees")
      stdout.put("Angle_abc/degrees")
      stdout.put("err/degrees")
      stdout.flush
      stdout.dash(int_fields=3,real_fields=3)

      ! Table
      do k = 1,n_k
      do kk = 1,size(atom_kind(k).element)

         a = atom_kind(k).element(kk)
         symbol_a = .list_symbol(a)

         do l = 1,n_k
         do ll = 1,size(atom_kind(l).element)

            b = atom_kind(l).element(ll)
            symbol_b = .list_symbol(b)

            if (a==b) cycle
            if (NOT print_all_angles AND NOT .bonded(a,b)) cycle

            do m = 1,n_k
            do mm = 1,size(atom_kind(m).element)

               c = atom_kind(m).element(mm)
               if (b==c OR c==a) cycle
               if (NOT print_all_angles AND NOT .bonded(b,c)) cycle

               ! Angle 
               theta_abc = .bond_angle(a,b,c)
               theta_abc.convert_to("degree")

               ! Extract covariances for atoms a, b, c
               covariance_matrix.put_blocks_to(cov,block_dim=3,block_list=[a,b,c])

               ! Make error
               .bond_angle_deriv(a,b,c,der,abc_colinear)
               if (abc_colinear) then
                  err = ZERO
               else
                  err = dot_product(der,matmul(cov,der))
                  err = sqrt(err)
                  err.convert_to("degree")
               end

               ! Print out
               symbol_c = .list_symbol(c)
               stdout.put(symbol_a,int_width=TRUE)
               stdout.put(symbol_b,int_width=TRUE)
               stdout.put(symbol_c,int_width=TRUE)
               stdout.put(theta_abc,err)
               stdout.put(theta_abc)
               if (abc_colinear) then; stdout.put("abc colinear")
               else;                   stdout.put(err)
               end
               stdout.flush

            end
            end

         end
         end

      end
      end
      stdout.dash(int_fields=3,real_fields=3)

      ! Clean & unsave
      atom_kind.destroy
      stdout.unsave

   end

   put_torsion_angle_table(covariance_matrix,all)
   ! Output the torsion angle table with standard errors given a
   ! "covariance_matrix" for the atom positions. If "all" is present
   ! and true, put out all possible angles
      covariance_matrix :: MAT{REAL}, IN
      all :: BIN, IN, optional

      print_all_angles,abc_colinear,bcd_colinear :: BIN
      atom_kind :: VEC{VEC_{INT}}*
      n_k,k,l,m,n,kk,ll,mm,nn,a,b,c,d :: INT
      symbol_a,symbol_b,symbol_c,symbol_d :: STR
      theta_abcd,err :: REAL
      der :: VEC{REAL}(12)
      cov :: MAT{REAL}(12,12)

      print_all_angles = FALSE
      if (present(all)) print_all_angles = all

      if (.dim<4) return
      if (.no_of_torsion_angles<1 AND NOT print_all_angles) return
      if (.no_of_angles>100 AND NOT print_all_angles) return

      ! Increase accuracy
      stdout.save
      stdout.set_real_width(stdout.saved.real_width+3)
      stdout.set_real_precision(stdout.saved.real_precision+3)

      ! Get atom kinds
      .make_atom_kind_list(atom_kind)
      n_k = size(atom_kind)

      ! Table header
      stdout.flush
      stdout.text("Torsion angles (looking down b->c):",flush=2)
      stdout.show("No. of independent angles =",.no_of_torsion_angles)
      stdout.flush
      stdout.dash(int_fields=4,real_fields=3)
      stdout.put("Atom_a",int_width=TRUE)
      stdout.put("Atom_b",int_width=TRUE)
      stdout.put("Atom_c",int_width=TRUE)
      stdout.put("Atom_d",int_width=TRUE)
      stdout.put("Angle_abcd/degrees")
      stdout.put("Angle_abcd/degrees")
      stdout.put("error/degrees")
      stdout.flush
      stdout.dash(int_fields=4,real_fields=3)

      ! Table
      do k = 1,n_k
      do kk = 1,size(atom_kind(k).element)

         a = atom_kind(k).element(kk)
         symbol_a = .list_symbol(a)

         do l = 1,n_k
         do ll = 1,size(atom_kind(l).element)

            b = atom_kind(l).element(ll)
            symbol_b = .list_symbol(b)

            if (a==b) cycle
            if (NOT print_all_angles AND NOT .bonded(a,b)) cycle

            do m = 1,n_k
            do mm = 1,size(atom_kind(m).element)

               c = atom_kind(m).element(mm)
               symbol_c = .list_symbol(c)

               if (a==c OR b==c) cycle
               if (NOT print_all_angles AND NOT .bonded(b,c)) cycle

               do n = 1,n_k
               do nn = 1,size(atom_kind(n).element)

                  d = atom_kind(n).element(nn)
                  symbol_d = .list_symbol(d)

                  if (NOT print_all_angles AND NOT .bonded(c,d)) cycle
                  if (a==d OR b==d OR c==d) cycle

                  ! Angle 
                  theta_abcd = .torsion_angle(a,b,c,d,abc_colinear,bcd_colinear)
                  theta_abcd.convert_to("degree")

                  ! Extract covariances for atoms a, b, c, d
                  covariance_matrix.put_blocks_to(cov,block_dim=3,block_list=[a,b,c,d])

                  ! Make error
                  .bond_angle_deriv(a,b,c,der,abc_colinear)
                  if (abc_colinear OR bcd_colinear) then
                     err = ZERO
                  else
                     err = dot_product(der,matmul(cov,der))
                     err = sqrt(err)
                     err.convert_to("degree")
                  end

                  ! Print out
                  stdout.put(symbol_a,int_width=TRUE)
                  stdout.put(symbol_b,int_width=TRUE)
                  stdout.put(symbol_c,int_width=TRUE)
                  stdout.put(symbol_d,int_width=TRUE)
                  if (abc_colinear AND bcd_colinear) then
                     stdout.put("a-b-c-d colinear")
                     stdout.flush
                  else if (abc_colinear) then
                     stdout.put("a-b-c colinear")
                     stdout.flush
                  else if (bcd_colinear) then
                     stdout.put("b-c-d colinear")
                     stdout.flush
                  else
                     stdout.put(theta_abcd,err)
                     stdout.put(theta_abcd)
                     stdout.put(err)
                     stdout.flush
                  end

               end
               end

            end
            end

         end
         end

      end
      end
      stdout.dash(int_fields=4,real_fields=3)

      ! Clean & unsave
      atom_kind.destroy
      stdout.unsave

   end

