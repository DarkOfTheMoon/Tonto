program run_MAT{REAL}

   implicit none

   m :: MAT{REAL}*

   TONTO_CREATE

   print *,"size =",sizeof(m)
   print *,"size =",sizeof(tonto)
   m.create(3,3)
   m.to_unit_matrix
   print *,m(1,1)
   print *,m(1,2)
   print *,m(2,2)
   print *,m(3,1)
   print *,m(3,2)
   print *,m(3,3)
   m.destroy

   TONTO_DESTROY

end

