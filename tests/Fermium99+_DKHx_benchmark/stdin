! see the README file for details
{

   name=   Fermium99+
   
   output_style_options= {
      real_precision= 4
   }
   
   basis_directory= .
   basis_name= basis-even    ! an even tempered series of gaussian 
   
   charge= 99
   multiplicity= 2
   
   atoms= {

      keys= { label= { units= angstrom } pos= }

      data= {
        Fm   0.000000    0.000000    0.000000
      }

   }

   put


   scfdata= {

      use_douglas_kroll_hess= TRUE
      use_DK1= TRUE
      use_DK2= TRUE
      use_DK3= TRUE
      initial_density= core 

      kind=            uhf

      direct=          on

      max_iterations= 10

      convergence= 0.00001
      diis= { convergence_tolerance= 0.00001 }

   }
   
   scf

   delete_scf_archives
   delete_scf_integrals

}
