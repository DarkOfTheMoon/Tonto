{

   name=   Fermium99+
   
   output_style_options= {
      real_precision= 8
   }
   
   basis_directory= . 
   basis_name= basis-even    ! an even tempered series of gaussian 
   
   charge= 99
   multiplicity= 2
   
   atoms= {

      keys= { label= { units= angstrom } pos= }

      data= {
        Fm   0.000000    0.000000    0.000000
      }

   }

   put


   scfdata= {

      use_douglas_kroll_hess= TRUE
      use_DK1= TRUE
      use_DK2= TRUE
      use_DK3= TRUE
      initial_density= core 

      kind=            uhf

      direct=          on

      convergence= 0.00000000001
      max_iterations= 10
      diis= { convergence_tolerance= 0.000000001 }
      delete_scf_archives= NO
     
   }
   
   scf
   put_mos_and_energies
}
