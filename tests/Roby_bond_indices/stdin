{
   name= h2co

   output_style_options= { real_precision= 3 }

   basis_set_kind= 6-31g-star
   
   atoms= {

      keys= { label= { units= angstrom } pos= }

      data= {
         8       0.000000   -0.676255    0.000000
         6       0.000000    0.530636    0.000000
         1       0.913322    1.113112    0.000000
         1      -0.913323    1.113110    0.000000
      }
   }

   put

   scfdata= {
      kind= rhf
      initial_density= promolecule
      converge= 0.000001
      diis= { convergence_tolerance= 0.000001 }
   }
   scf


   ! Roby population analysis

   robydata= {
      kind= atom_bond_analysis
      atom_list= { 1 ... 4 }
      analyse_all_atom_pairs= TRUE
    ! covalent_cutoff = 89 degree
   }

   roby_analysis


   robydata= {
      kind= group_bond_analysis
      atom_groups= { { 1 } { 2 3 4 } }
    ! covalent_cutoff = 89 degree
   }

   roby_analysis

   delete_scf_archives
   delete_scf_integrals

}
